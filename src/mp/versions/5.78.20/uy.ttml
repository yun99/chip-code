<button class="custom-class bitc-btn {{inGroup ? 'bitc-btn--group' : ''}} {{isLast ? 'bitc-btn--last' : ''}} {{size ? 'bitc-btn--'+size : ''}} {{plain ? 'bitc-btn--plain' : 'bitc-btn--solid'}} {{type ? 'bitc-btn--'+type : ''}} {{loading ? 'bitc-btn--loading' : ''}} {{disabled ? 'bitc-btn--disabled' : ''}} {{inline ? 'bitc-btn--inline' : ''}}" disabled="{{disabled}}" hover-class="button-hover" style="{{customStyle}};{{otherStyle}}" open-type="{{openType}}" app-parameter="{{appParameter}}" hover-stop-propagation="{{hoverStopPropagation}}" hover-start-time="{{hoverStartTime}}" hover-stay-time="{{hoverStayTime}}" lang="{{lang}}" session-from="{{sessionFrom}}" send-message-title="{{sendMessageTitle}}" send-message-path="{{sendMessagePath}}" send-message-img="{{sendMessageImg}}" show-message-card="{{showMessageCard}}" bindtap="handleTap" bindcontact="bindcontact" bindgetuserinfo="bindgetuserinfo" bindgetphonenumber="bindgetphonenumber" bindopensetting="bindopensetting" binderror="binderror" data-obj="{{extendData}}"><slot></slot></button>