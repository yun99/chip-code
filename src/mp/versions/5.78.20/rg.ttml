<view class="no-data-container" style="{{customContainerStyle}}"><wq class="no-data-image" src="{{customImage || image}}" webp-conf="{{customPicConf}}" styleStr="{{customPicStyle}}" tt:if="{{type !== 'NETWORK'}}" /><view class="no_network_image" tt:else /><text class="no-data-title g-color-1 g-text-1" tt:if="{{title}}">{{title}}</text><text class="no-data-tips g-color-3 g-text-3" style="{{customTipsStyle}}" tt:if="{{tips}}">{{tips}}</text><view class="no-data-btn-wrap"><view class="no-data-btn-{{from}} no-data-action g-text-1" bind:tap="goAction" style="{{customBtnStyle || ''}}" tt:if="{{btnText && showBtn}}">{{btnText}}</view><view class="no-data-btn-{{from}} no-data-sub-action g-font-bold" bind:tap="goKefu" tt:if="{{subBtnText}}">{{subBtnText}}</view><view class="suggestions-list g-font-bold" bind:tap="goSuggestionList" tt:if="{{suggestionsLink && suggestionsText}}">{{suggestionsText}}</view><slot name="btn"></slot></view><view class="mall-error-code" tt:if="{{errorCode}}">{{errorCode}}</view></view>