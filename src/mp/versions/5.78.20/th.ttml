<wxs src="./i0.sjs" module="asset"></wxs><tf show="{{show}}" type="bottom" show-mask="{{show}}" is-full-screen="{{true}}" z-index="1000" catchtouchmove tt:if="{{show}}"><view class="self-lifting-confirm-wrp"><view class="header"><view class="title">{{selfTipStrs[0]}}<view class="red" tt:if="{{selfTipStrs[1]}}">{{selfTipStrs[1]}}</view></view><view class="title" tt:if="{{selfTipStrs[2]}}">{{selfTipStrs[2]}}</view><image class="close" src="{{asset('images/close_gary')}}" bindtap="toClose"></image></view><view class="self-lifting-confirm-container"><view class="address-pic"><wq src="{{selfLifting.selfLiftingAddressPic}}" webp-conf="{{({width: '140rpx', height: '140rpx'})}}" /></view><view class="name">{{selfLifting.selfLiftingAddressName}}</view><view class="address-wrapper"><view class="self-tip" tt:if="{{selfLifting.selfDistanceStr}}">距您<view class="red">{{selfLifting.selfDistanceStr}}</view><view class="divide" /></view><view class="address">{{selfLifting.selfLiftingAddress}}</view></view></view><view class="footer"><re class="btn" type="primary" other-style="background: #FFFFFF;font-weight: 400;font-family: PingFang SC;border: 2rpx solid #eee;" bindtap="switch">切换自提点</re><re class="btn" other-style="background: #FFD100;font-weight: 400;font-family: PingFang SC;" type="warn" bindtap="toConfirm">确认并支付</re></view></view></tf>