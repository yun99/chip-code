.body{background-clip:content-box;background-color:#f5f6f7;box-sizing:border-box;display:flex;flex-direction:column;height:100vh;overflow:hidden;padding-bottom:env(safe-area-inset-bottom)}.header{position:relative;z-index:2}.mask{background:rgba(0,0,0,.8);bottom:0;left:0;position:fixed;right:0;top:0;z-index:3}.header-fixed-top-wrapper{bottom:2rpx;left:0;position:absolute;transform:translateY(100%);width:100%;z-index:10}.content-scroll-wrapper{display:flex;flex-direction:column;flex-grow:1;flex-shrink:1;height:1px;overflow:scroll;overflow-anchor:auto;position:relative;z-index:2}.show-mask-content-scroll-wrapper{z-index:4}.content-box{flex-grow:1;flex-shrink:1}.bottom-box{background-color:#fff;position:relative;z-index:2}.unkeep-out-bottom-box{bottom:100%;position:absolute}.keep-out-bottom-box{z-index:9}.alert{z-index:99}.refresh-box{height:0;width:100%}.refresh-dot-box{align-items:center;background-color:#ffd100;display:flex;height:80px;justify-content:center;padding-top:80px;transform:translateY(-80px);width:100%}.refresh-dot{background-color:rgba(0,0,0,.15);border-radius:50%;height:14rpx;margin:0 5rpx;width:14rpx}.refresh-dot.pulling{animation:refreshingDot 2.4s linear infinite}.refresh-dot.pulling.center{animation-delay:.8s}@keyframes refreshingDot{0%{background-color:rgba(0,0,0,.15)}50%{background-color:rgba(0,0,0,.45)}to{background-color:rgba(0,0,0,.15)}}