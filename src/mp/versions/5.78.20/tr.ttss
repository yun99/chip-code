.bitc-toast{background:rgba(0,0,0,.8);border-radius:20rpx;box-sizing:border-box;color:#fff;font-family:PingFangSC-Medium;font-size:32rpx;left:50%;line-height:45rpx;margin:0 auto;padding:30rpx 40rpx;position:fixed;text-align:center;top:50%;transform:translate3d(-50%,-100%,0);z-index:2050}.bitc-toast_content{align-items:center;display:flex;justify-content:center}.bitc-toast--notitle{padding:40rpx}.bitc-toast__icon{display:block;font-size:38rpx;height:80rpx;line-height:80rpx;margin:0 auto;padding:30rpx;text-align:center;width:80rpx}.bitc-toast__icon-loading{line-height:0}.bitc-toast__message{max-width:373rpx;white-space:pre-wrap}.bitc-loading{animation:weuiLoading 1s steps(12) infinite;background:transparent url(data:image/svg+xml;base64,) no-repeat;background-size:100%;display:inline-block;height:40rpx;vertical-align:middle;width:40rpx}.bitc-toast__icon-loading .bitc-loading{height:80rpx;width:80rpx}.bitc-toast__icon-image{background-position:50%;background-repeat:no-repeat;background-size:80rpx}.bitc-toast__error_code{color:hsla(0,0%,100%,.3);display:block;font-size:24rpx;text-align:center}