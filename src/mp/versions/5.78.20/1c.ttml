<wxs module="skuBus" src="./ji.sjs"></wxs><template name="abs-element"><block tt:if="{{options.eleType === 'topLeftTag' && skuBus.topLeftTagInfo(sku, dataPath.default, options, '.img')}}"><wq class="ps_a {{classStr}}" isBlockImg="{{true}}" webp-conf="{{options.webpConf ? (skuBus.unUseWebp(options.webpConf)) : options.sizeAuto ? ({ isAuto: true, quality: 100, useWebp: false }) : ({ width: options.width, height: options.height, quality: 100, useWebp: false })}}" src="{{skuBus.topLeftTagInfo(sku, dataPath.default, options, '.img')}}" /><view class="ratioDesc {{innerClass.ratioDesc || skuBus.getRadioTextSize(options.webpConf.width)}}" tt:if="{{skuBus.getDataByPath(sku, dataPath.default).type === 1}}"><text class="mr_6" tt:if="{{skuBus.getDataByPath(sku, dataPath.default).tagText}}">{{skuBus.getDataByPath(sku, dataPath.default).tagText}}</text><text tt:if="{{skuBus.getDataByPath(sku, dataPath.default).extraInfo}}">{{skuBus.getDataByPath(sku, dataPath.default).extraInfo}}</text></view><view class="promotionTag" tt:elif="{{skuBus.getDataByPath(sku, dataPath.default).type === 4}}"><view class="promotionTag-text {{innerClass.promotionTag}}">{{skuBus.getDataByPath(sku, dataPath.default).text}}</view></view></block><view class="ps_a sell-out-layer {{classStr}}" tt:elif="{{options.eleType === 'soldOutMark' && skuBus.getDataByPath(sku, dataPath.default)}}"><wq isBlockImg="{{true}}" webp-conf="{{options.webpConf ? (skuBus.unUseWebp(options.webpConf)) : options.sizeAuto ? ({ isAuto: true, quality: 100, useWebp: false }) : ({ width: options.width, height: options.height, quality: 100, useWebp: false })}}" src="{{sku.soldOutUrl}}" /></view><block tt:elif="{{options.eleType === 'priceTag' && skuBus.transBottomTag(sku).text}}"><view class="ps_a {{classStr}}"><view class="bottom-promotion"><view class="bottom-promotion-icon"><view class="bottom-promotion-tag {{innerClass.bottomPromotionTag}}">{{skuBus.transBottomTag(sku).iconText}}</view></view><view class="bottom-promotion-info {{innerClass.bottomPromotionText}}">{{skuBus.transBottomTag(sku).text}}</view></view></view></block><block tt:elif="{{options.eleType === 'promotionAdvance' && skuBus.getDataByPath(sku, dataPath.default)}}"><view class="ps_a promotion-advance {{classStr}}"><wq isBlockImg="{{true}}" class="promotion-advance__bg" webp-conf="{{options.webpConf ? (options.webpConf) : ({ isAuto: true })}}" src="https://p0.meituan.net/groceryimages/74042d018a9bc354bc3a1bd3321187204670.png" /><view class="promotion-advance__time {{innerClass.promotionTime}}">{{skuBus.getDataByPath(sku, dataPath.default).startTime.text}}</view><view class="promotion-advance__price"><view class="{{innerClass.promotionTag}}">{{skuBus.getDataByPath(sku, dataPath.default).priceTag.text}}</view><view class="{{innerClass.promotionPrice}}">{{skuBus.getDataByPath(sku, dataPath.default).price.text}}</view></view></view></block><wq isBlockImg="{{true}}" class="ps_a {{classStr}}" webp-conf="{{options.webpConf ? (options.webpConf) : options.sizeAuto ? ({ isAuto: true }) : ({ width: options.width, height: options.height })}}" src="{{skuBus.getDataByPath(sku, dataPath.default)}}" tt:elif="{{options.eleType === 'bitc-image' && skuBus.getDataByPath(sku, dataPath.default)}}" /></template>