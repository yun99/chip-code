@import "./jn.ttss";.root{flex-direction:column;height:100%;position:relative}.card{border-radius:18rpx;margin:18rpx 18rpx 0;overflow:hidden}.unfill-style{background:#fafafa;margin-bottom:14rpx}.fill-style{margin-top:50%;transform:translateY(-50%)}.gradient{height:100rpx;left:0;position:absolute;top:0;width:100%}.gradient-head{background-image:linear-gradient(180deg,#fafafa,#f0f1f2)}.gradient-bottom{background-image:linear-gradient(180deg,#f0f1f2,#fff);top:-100rpx;z-index:-10}.gradient-bottom_empty{background-image:linear-gradient(180deg,#fafafa,#fff)}.view-price-btn{align-items:center;background:rgba(0,0,0,.7);border-radius:12rpx;bottom:36rpx;box-shadow:0 2rpx 8rpx 0 rgba(0,0,0,.08);color:#fff;display:inline-flex;font-size:28rpx;line-height:40rpx;padding:14rpx 18rpx;position:absolute}