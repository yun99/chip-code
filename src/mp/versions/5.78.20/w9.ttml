<wxs module="asset" src="./ot.sjs" /><template name="board-list"><view class="sku-item" tt:if="{{skuList && skuList.length}}" tt:for="{{skuList}}" tt:key="skuId"><ux index="{{index}}" sku="{{({...item, sku_index: index})}}" type="{{skuType}}" schema="{{skuSchema}}" canDirect="{{!isCustomPicClick}}" proxyData="{{proxyData}}" bind:gotoSkuDetail="gotoSkuDetail" bind:getRecSku="handleGetRecSku" bind:addToCartCompletely="onAddToCartCompletely" /><view class="sku-item-tag {{index >= 3 ? 'num-bottom' : ''}}" tt:if="{{hasNumTag || type == 4}}">{{index + 1}}<wq class="sku-item-tag" style="z-index: -1;" isBlockImg src="{{index < 4 ? tagImagesOver[index] : tagImagesOver[3]}}" webp-conf="{{({width:'41rpx', height:'48rpx'})}}" /></view></view></template><view class="sku-board external-class" style="{{customStyle}}"><view class="head" tt:if="{{title.length || subTitle.length}}"><view class="head-title"><view class="main-title g-text-1 g-font-bold g-color-1" bind:tap="clickTitle">{{title}}</view><view class="sub-title g-text-3 g-color-3" bind:tap="clickSubTitle"><slot name="sub-title-{{idx}}"></slot>{{subTitle}}</view></view><view class="see-more-wrap" bind:tap="seeMore" data-button-position="head" tt:if="{{skuSeeMore && skuSeeMore.iretail}}"><view class="see-more-name g-text-3 g-color-4">{{moreText || '查看全部'}}</view><wq src="https://p1.meituan.net/groceryimages/e8737c269592470d3321e2320892f6cf297.png" isBlockImg class="see-more-icon" webp-conf="{{({width:'22rpx', height:'22rpx'})}}" /></view></view><view class="sku-content" tt:if="{{!isScrollBoard}}"><template is="board-list" data="{{skuList, hasNumTag, skuSchema, isCustomPicClick, proxyData, hasNumTag, tagImagesOver, skuType}}" /></view><scroll-view class="sku-content {{isScrollBoard ? 'sku-centent-scroll' : ''}}" scroll-x enhanced show-scrollbar="{{false}}" tt:else><template is="board-list" data="{{skuList, hasNumTag, skuSchema, isCustomPicClick, proxyData, hasNumTag, tagImagesOver, skuType}}" /><view class="sku-content-showMore" bind:tap="seeMore" data-button-position="right" tt:if="{{( !hideLongTextButton || hideLongTextButton && !(rightSkuSeeMore && rightSkuSeeMore.text && rightSkuSeeMore.text.length > 7) )}}">{{rightSkuSeeMore && rightSkuSeeMore.text || '查看全部'}}<wq class="bitc" webp-conf="{{({width:'22rpx', height: '22rpx'})}}" src="{{asset('images/home/cboard-seek-all')}}" /></view><view class="sku-content-showMore-right"></view></scroll-view></view>