<wxs module="fps" src="../../../ij.sjs" /><view class="form-container"><view class="floor"><input placeholder-class="placeholder" placeholder="请输入手机号" type="number" maxlength="11" bindinput="phoneInputHandler" focus="true" class="phone-input text-input" /><button tt:if="{{true}}" class="wx-userinfo-btn code-btn" style="{{sendCodeBtn.active ? configData.sendCodeActiveStyle : ''}}" bindtap="sendVerifyCodeClick" hidden="{{!countdown.hidden}}">获取验证码</button><text tt:else class="code-btn" style="{{sendCodeBtn.active ? configData.sendCodeActiveStyle : ''}}" bindtap="sendVerifyCodeClick" hidden="{{!countdown.hidden}}">获取验证码</text><text class="code-btn" style="{{activeStyle}}" hidden="{{countdown.hidden}}">{{countdown.limit}}秒</text></view><view class="floor"><input placeholder-class="placeholder" type="number" class="code-input text-input" maxlength="6" placeholder="请输入验证码" bindinput="mobileCodeInputHandler" /></view><button class="wx-userinfo-btn submit" style="{{submitBtn.active ? configData.loginActiveStyle : ''}}" open-type="getUserInfo" bindtap="loginClick">{{configData.loginText}}</button></view><view tt:if="{{showVoiceTip}}" class="voice-tip"><text style="{{configData.voiceTipStyle}}">{{configData.voiceTipText}}</text></view><view class="protocol-container" tt:if="{{protocolConfig.show}}"><text style="{{protocolConfig.style}}">{{protocolConfig.preText}}</text><block tt:for="{{protocolConfig.protocolList}}" tt:key="{{id}}"><navigator url="../webview/index?src={{item.encodedUrl}}" style="{{item.style}}" hover-class="none">{{item.text}}</navigator><block tt:if="{{index < protocolConfig.protocolList.length - 1}}">{{protocolConfig.separator}}</block></block></view><yj id="slider" title="滑块验证" bind:sliderEvent="onSliderEnd"></yj><yk id="image" title="图片验证" verifyText="验证" bind:imageEvent="onImageEnd"></yk><view class="second-mobile-wrapper" tt:if="{{secondMobile.pageShow}}"><view class="second-mobile-tip">检测到当前登录环境异常，为保障账号安全，请确认这是不是您本人的账号</view><view class="second-mobile-avatar"><image src="{{secondMobile.imgSrc}}" /></view><view class="second-mobile-nickName">{{secondMobile.userName}}</view><button class="second-mobile-confirm" bindtap="onSecondMobileYesClick">是我的账号</button><button class="second-mobile-deny" bindtap="onSecondMobileNoClick">不是我的账号</button><view class="second-mobile-tip-2">不是我的账号？可能是前任手机号主人注册的。点击“不是我的账号”，将为您注册新的账号</view></view><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>