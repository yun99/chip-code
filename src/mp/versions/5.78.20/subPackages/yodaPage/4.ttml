<wxs module="label" src="./4.sjs"></wxs><view id="yoda-inference" class="inference-wrap" style="display:{{isShow}}" bindtouchstart="onGlobaTouchstart" bindtouchend="onGlobaTouchend"><view class="title-wrap"><text class="title">{{message}}</text><image src="{{refreshIcon}}" class="{{rotateClass}}" style="width: 24px; height: 24px;" catchtap="onrefresh"></image><image src="{{closeIcon}}" style="width: 24px; height: 24px;" catchtap="onclose"></image></view><view class="img-wrap" style="height: 35px;" tt:if="{{msgType === -1}}"><image src="{{titleUrl}}" class="tip-img" /></view><view class="succ-wrap tip-wrap" tt:if="{{msgType === 1}}"><icon class="icon-small" type="success" size="20"></icon><text class="tip tip-ok">验证通过</text></view><view class="wran-wrap tip-wrap" tt:if="{{msgType === 2}}"><icon class="icon-small" type="warn" size="20" color="#ffc300"></icon><text class="tip tip-warn">{{errorMsg}}</text></view><view class="err-wrap tip-wrap" tt:if="{{msgType === 0}}"><icon class="icon-small" type="cancel" size="20"></icon><view style="margin-left: 6px;"><text class="tip tip-err" style="margin-left: 0px;">{{errorMsg}}</text></view></view><view bindtap="{{label.ontap}}" class="content-wrap" change:prop="{{label.propObserver}}" prop="{{wxsProp}}"><image id="yoda-img" src="{{imgurl}}" style="width: 268px; height: 178px; z-index: 1; border-radius: 16rpx;" bindtouchstart="ontouchstart" bindtouchend="ontouchend"></image><text data-id="1" id="movable_1" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">1</text><text data-id="2" id="movable_2" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">2</text><text data-id="3" id="movable_3" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">3</text><text data-id="4" id="movable_4" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">4</text><text data-id="5" id="movable_5" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">5</text><text data-id="6" id="movable_6" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">6</text><text data-id="7" id="movable_7" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">7</text><text data-id="8" id="movable_8" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">8</text><text data-id="9" id="movable_9" class="label" bindtouchend="{{label.oncancel}}" catchtap="oncancel">9</text></view><view tt:if="{{category === 'GROUP'}}"><text bindtap="changeVerify" class="yoda_change_text">切换验证方式</text></view><canvas type="2d" id="canvas" width="536" height="356" style="position: fixed; left: 1200rpx; top: 0rpx;"></canvas></view>