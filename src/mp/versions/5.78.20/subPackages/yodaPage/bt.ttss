.inference-wrap{background:#fff;border-radius:10px;padding:12px 8px;width:268px}.inference-wrap .title{color:rgba(0,0,0,.84);font-family:PingFangSC-Medium;font-size:16px;font-weight:500;line-height:24px;width:222px}.inference-wrap .title-wrap{display:flex;width:268px}.inference-wrap .img-wrap{line-height:35px}.inference-wrap .img-wrap,.inference-wrap .tip-wrap{background:#f5f5f5;border-radius:8px;margin-top:8px;width:268px}.inference-wrap .tip-wrap{align-items:center;display:flex;padding:6.5px 0}@keyframes a{to{transform:rotate(1turn)}}.inference-wrap .animation-rotate{animation:a .8s linear infinite}.inference-wrap .icon-small{height:20px;margin-left:8px}.inference-wrap .succ-wrap{background:#edfaf3}.inference-wrap .wran-wrap{background:#fff9e6}.inference-wrap .err-wrap{background:#fff5f4}.inference-wrap .tip{font-family:PingFangSC-Regular;font-size:14px;font-weight:400;height:22px;line-height:22px;margin-left:6px}.inference-wrap .tip-ok{color:#005238}.inference-wrap .tip-warn{color:#592d00}.inference-wrap .tip-err{color:#660e16}.inference-wrap .tip-img{height:35px;width:268px}.inference-wrap .content-wrap{background:#f5f5f5;border-radius:8px;height:178.5px;margin-top:8px;position:relative;width:268px}.inference-wrap .label{background-color:#0a70f5;border:2px solid #fff;border-radius:50%;color:#fff;display:none;font-size:14px;line-height:20px;position:fixed;text-align:center;width:20px;z-index:1}.inference-wrap .yoda_change_text{color:#fe8c00;display:inline-block;font-size:14px;margin:10px 0;text-align:center;width:100%}