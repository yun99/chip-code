<view class="map-guide-mask" tt:if="{{!hasShown && poiItem.poiName && cardTop && !overMaxPageNum}}"><view class="page first-step" catchtap="goToNextStep" tt:if="{{currentStep === 1}}"><cover-image class="page-1-text-1" style="top: {{textTop}}rpx" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/map_text_1.9a89422.png" /><view class="container container-un-selected" style="top: {{cardTop}}rpx"><view class="pic-wrapper"><cover-image class="avatar" src="{{poiItem.picUrl}}" /><view class="stop-business-tip" tt:if="{{state === 6}}">团长关店</view></view><view class="right-wrapper right-wrapper-un-selected break-line"><view class="rightSection break-line"><view class="right-top break-line"><view class="break-line"><view tt:if="{{needHighlight}}"><view class="name-hight {{item.highLight && poiItem.poiBusinessStatus === 1 ? 'name-hight-enable' : 'name-disable'}}" tt:for="{{poiItem.lightTxt}}" tt:key="index">{{item.text}}</view></view><view tt:else><view class="name name-un-selected">{{poiItem.poiName}}</view></view><view class="tips-wrapper break-line" tt:if="{{poiItem.poiLabels && poiItem.poiLabels.length}}"><view class="item" style="color: {{item.fontColor}}; border: solid 1rpx {{item.labelColor}}" tt:for="{{poiItem.poiLabels}}">{{item.labelName}}</view></view><view class="bottom-info-wrapper break-line"><view class="desc break-line" tt:if="{{poiItem.distanceText || poiItem.tag}}"><view class="tag" tt:if="{{poiItem.tag}}">{{poiItem.tag}}</view><view class="text text-un-selected" tt:if="{{poiItem.distanceText}}">{{poiItem.distanceText}}</view></view><view class="vertical-line break-line" tt:if="{{poiItem.distanceText && poiItem.address}}"></view><view class="addr break-line" tt:if="{{poiItem.address}}">{{poiItem.address}}</view></view></view><view class="btn-wrap btn-wrap-un-selected"><cover-image class="radio" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/radio.f9141067.png" /></view></view></view><view class="range-wrapper" catchtap="gotoRange" tt:if="{{!isMTTiny && poiItem.poiDeliveryInfo && poiItem.poiDeliveryInfo.deliveryRange}}"><view class="label">{{poiItem.poiDeliveryInfo.deliveryTypeText || '送货上门'}}</view><view class="line"></view><view class="range-text">{{poiItem.poiDeliveryInfo.deliveryRange}}</view><view class="range-arrow-wrap"><cover-image src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/range_right_arrow.e9d2558.png" /></view></view></view></view><view class="next-button" style="top: {{btnTop}}rpx">下一步</view></view><view class="page second-step" catchtap="goToNextStep" tt:if="{{currentStep === 2}}"><cover-image class="page-2-pic-1" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/map_pic_2.68e9417.png" /><cover-image class="page-2-pic-2" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/map_text_2.709d423.png" /><view class="next-button" catchtap="goToNextStep">我知道了</view></view></view>