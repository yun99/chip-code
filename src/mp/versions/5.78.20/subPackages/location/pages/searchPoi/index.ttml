<wxs module="fps" src="../../../../ij.sjs" /><view class="page-container"><view class="container" style="position: {{showSearchList ? 'fixed' : 'static'}}; z-index: 200"><include src="../../../../er.ttml" /><view class="search-bar-wrap" style="{{isSearchPage ? 'transform:translateX(-6rpx);' : ''}}"><uq id="searchPoiSearchBar" bindsetSearchList="setSearchList" bindshowCityList="showCityList" bindchangeCity="changeCity" fromTo="{{fromTo}}" focusin="{{focusin}}" cityAddress="{{cityAddress}}" page='selfPickSelect' position="{{position}}" showCityListView="{{showCityListView}}"></uq></view><yb show="{{!showCityListView && showSearchList}}" poi-list="{{searchResult.poiList}}" address-list="{{searchResult.addrList}}" bindpoiCardTap="searchPoiCardTap" bindchangeAddress="onSearchAddrTap"></yb><yn id="citySelector" is-show="{{showCityListView}}" selectLocation="{{selectLocation}}" bindcitySelect="onCitySelect" defaultCity="{{cityAddress}}" bindonCitySelectorClose="onCitySelectorClose"></yn><ym id="poi-dialog" /></view><view class="search-poi-root"><block tt:if="{{historyAddress.length || historyPoi.length}}"><view class="poi-heap" tt:if="{{historyAddress.length}}"><view class="flex-center poi-heap_title">地址搜索记录</view><view class="flex-center poi-heap_poi"><view class="flex-center poi-heap_poi_box" bind:tap="searchResultCheckAddress" data-value="{{item.value}}" data-index="{{index}}" tt:for="{{historyAddress}}" tt:if="{{item.name}}"><wq src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/search-poi-loaction.ae011082.png" webp-conf="{{({width:'26rpx', height:'26rpx'})}}" styleStr="display:block;margin-right:6rpx;" />{{item.name}}</view></view></view><view class="poi-heap" tt:if="{{historyPoi.length}}"><view class="flex-center poi-heap_title">自提点搜索记录</view><view class="flex-center poi-heap_poi"><view class="flex-center poi-heap_poi_box" bind:tap="searchResultCheckPoi" data-value="{{item.value}}" data-index="{{index}}" tt:for="{{hi