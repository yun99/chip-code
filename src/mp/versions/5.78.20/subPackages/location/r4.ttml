<view class="searchBar {{page}}"><view class="searchBox {{isHideBar?'hideSearchBox':'showSearchBox'}}"><view class="cityBox" bindtap="changeCity"><text class="cityBox-title lineDot">{{cityAddress.districtName || cityAddress.cityName||'选择城市'}}</text><image class="cityBox-dot{{showCityListView?' show':''}}" mode="scaleToFill" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/pull-down-19-8-22.5432541.png"></image></view><view class="inputBox"><image class="searchPic" mode="scaleToFill" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/search-19-8-21.bd5a647.png"></image><view class="input {{((focusin && page !== 'selfLifting') || (page === 'selfLifting' && showSearchList)) ?'focus':''}}"><input class="input-inline" data-from="wri" bindinput="bindKeyInput" focus="{{focusin}}" bindfocus="bindKeyFocus" value="{{keyword}}" bindconfirm="confirm" placeholder="{{placeHolder}}" placeholder-style="{{page === 'selfPickSelect' || page === 'otherSelfPickSelect' ? 'color: #95989E' : ''}}" bindblur="bindKeyBlur" confirm-type="search" /><view class="search-icon-clear-wrap {{((focusin && page !== 'selfLifting') || (page === 'selfLifting' && showSearchList)) ? '' : 'offset-right'}}" bindtap="clearInput" bindtouchstart="handleClearIconTouchStart"><image class="search-icon-clear" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/search_icon_clear.d8ad645.png" tt:if="{{isShowIconClear}}" /></view></view><view class="cancel {{((focusin && page !== 'selfLifting') || (page === 'selfLifting' && showSearchList))?'':'hidden'}}" bindtap="cancelSearch">取消</view></view></view><block tt:if="{{page !== 'selfPickSelect' && page !== 'otherSelfPickSelect' && page !== 'searchPoi'}}"><view class="fromTo lineDot {{isHideBar?'fromTop':''}}" tt:if="{{page !== 'selfLifting'}}">{{fromTo=='定位失败'?'请手动选择收货地址':'已选地址：'+fromTo}}</view><view class="fromTo self-lifting lineDot {{isHideBar?'fromTop':''}}" tt:elif="{{!showSearchList}}"><block tt:if="{{fromTo!='定位失败'}}"><image class="loc-img" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/submit_location.ad9b770.png"></image><view class="light-text">{{fromTo}}</view></block><view class="text">附近自提站点</view></view><view class="fromTo self lineDot {{isHideBar?'fromTop':''}}" bindtap="clickMyLoc" tt:else><image class="loc-img" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/submit_location.ad9b770.png"></image><view class="text">我的位置：{{currentAddress.address}}</view></view></block></view>