@keyframes rotate{0%{transform:rotate(0deg)}to{transform:rotate(1turn)}}.address-list-container{color:#585d66;font-size:28rpx;height:100%}.address-list-container .addr-list-wrap{height:calc(100vh - 745rpx)}.address-list-container .addr-list-wrap-no-map{height:calc(100vh - 745rpx + 480rpx)}.address-list-container .lineDot{overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.address-list-container .title{height:76rpx;line-height:76rpx;padding-left:30rpx;padding-top:10rpx}.address-list-container .item{align-items:center;background:#fff;border-bottom:1rpx solid #e1e1e1;display:flex;margin-left:30rpx;overflow:hidden;padding:24rpx 30rpx 24rpx 0}.address-list-container .item .content{color:#333;flex:1;font-size:28rpx;overflow:hidden}.address-list-container .item .content.disabled .address{color:#999}.address-list-container .item .content.disabled .tips{color:#999;font-size:26rpx;line-height:37rpx;margin-top:4rpx}.address-list-container .item .content .address{color:#121924;display:block;font-size:32rpx;line-height:45rpx;width:100%}.address-list-container .item.last{border-bottom:none}