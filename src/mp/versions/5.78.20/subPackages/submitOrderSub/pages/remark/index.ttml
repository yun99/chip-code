<wxs module="fps" src="../../../../ij.sjs" /><view class="remark-page"><include src="../../../../er.ttml" /><view class="text-content"><textarea disable-default-padding="{{true}}" class="textarea" placeholder-class="placeholder" placeholder="请填写备注，最多 50 个字" maxlength="{{50}}" bindinput="onOtherTextChange" bindfocus="textAreaFocus" value="{{otherText}}" hold-keyboard="{{true}}" focus="{{!isHaveHistoryMsg && isOnPage}}" /><view class="place">{{otherText.length}}/50</view></view><view class="easy-input"><view class="title" tt:if="{{isHaveHistoryMsg || isHaveRecommendRemarks}}"><text>快捷输入</text><view class="edit-butt" bindtap="editMsg" tt:if="{{isHaveHistoryMsg}}">{{isEditMessage ? '保存' : '编辑'}}</view></view><view class="input-items"><view class="input-item" bindtap="quickInputMsg" data-message="{{item}}" data-type="用户自定义" data-index="{{index}}" tt:for="{{remindMsg}}" tt:key="{{item}}"><view class="label">{{item}}</view><image class="delete-butt" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/message-delete.3adb1000.png" catchtap="deleteMsg" data-message="{{item}}" tt:if="{{isEditMessage}}" /></view><view class="input-item" bindtap="quickInputMsg" data-message="{{item}}" data-type="系统推荐" data-index="{{index}}" tt:for="{{recommendRemarks}}" tt:key="{{item}}"><view class="label">{{item}}</view></view></view></view><view class="button"><yh type="primary" bindtap="complete" custom-class="btn">完成</yh></view></view><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>