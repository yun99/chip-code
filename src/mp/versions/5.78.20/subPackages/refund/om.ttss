.page_base_class,page{background:#f5f6f7;height:auto;padding-bottom:110rpx}.section{background:#fff;border-radius:18rpx;margin:20rpx 24rpx;padding:24rpx 30rpx}.section .detail,.section .title{color:#121924;font-size:28rpx}.section .desc{color:#999;font-size:24rpx}.sub-section{margin:0!important;padding:0!important;width:100%}.row{align-items:center;display:flex;height:120rpx;justify-content:space-between}.min-row{height:88rpx}.bottom-bar{align-items:center;background:#fff;border-top:1rpx solid #d5d5d5;bottom:0;box-sizing:border-box;display:flex;height:110rpx;justify-content:space-between;padding:0 20rpx;position:fixed;width:100%;z-index:1}.btn-primary{background-image:linear-gradient(136deg,#ffd100,#ffd100);border-radius:100rpx;color:#fff;font-size:30rpx;height:80rpx}.btn-primary.disabled{background-image:linear-gradient(-143deg,#d9d9d9,#c9c9c9)}