.img-uploader{display:flex;flex-wrap:wrap;margin:0 20rpx 20rpx}.img-uploader.wrap{overflow:scroll}.img-uploader .uploading-tip{align-items:center;box-sizing:border-box;color:#c3c5c7;display:flex;font-size:28rpx;height:120rpx;justify-content:center;letter-spacing:0;margin:20rpx 20rpx 0 0}.img-item.last{margin:0}.img-item .img-add,.img-item .img-view{border-radius:14rpx;box-sizing:border-box;height:130rpx;margin:16rpx 16rpx 0 0;position:relative;width:130rpx}.img-item .img-add{align-items:center;background:#fff;border:2rpx dashed #eee;border-radius:12rpx;box-sizing:border-box;display:flex;flex-direction:column;justify-content:center}.img-item .img-add .btn-add{height:70rpx;width:70rpx}.img-item .img-add .txt-add{color:rgba(0,0,0,.5);font-size:24rpx;line-height:34rpx}.img-item .img-add .txt-count{color:#585d66;font-size:24rpx;line-height:24rpx;margin-top:11rpx}.img-item .icon-remove{height:40rpx;position:absolute;right:-8rpx;top:-14rpx;width:40rpx;z-index:1}.img-item .img-self{border-radius:10rpx;max-height:100%;max-width:100%;position:absolute}.img-item .fail-mask{align-items:center;background:rgba(0,0,0,.5);border-radius:10rpx;color:#fff;display:flex;flex-direction:column;font-size:20rpx;height:100%;justify-content:center;left:0;position:absolute;top:0;width:100%}