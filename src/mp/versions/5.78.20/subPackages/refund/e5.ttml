<view class="img-uploader {{wrap ? 'wrap' : ''}}" style="{{customStyle}}"><view class="img-item {{size}}" tt:for="{{itemList}}" tt:key="{{index}}"><view class="img-view" data-index="{{index}}" data-url="{{item.tempUrl}}" catchtap="onImgClick" tt:if="{{item._type !== 'add'}}"><image class="icon-remove" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon-del.c2f9363.png" data-index="{{index}}" catchtap="toRemoveImg"></image><image class="img-self" src="{{item.tempUrl}}"></image><view class="fail-mask" tt:if="{{item.state < 0}}"><view>图片</view><view>上传失败</view></view></view><view class="img-add" style="{{imgAddStyle}}" catchtap="toUploadImg" tt:elif="{{itemList.length < limit + 1}}"><image class="btn-add" style="{{btnAddStyle}}" src="{{imgAddIcon || 'https://p0.meituan.net/groceryimages/9afd3ff5e3a47cfd467e42e940d1c999562.png'}}"></image><block tt:if="{{itemList.length <= 1}}"><view class="txt-add" style="{{textAddStyle}}">上传照片</view></block><block tt:if="{{itemList.length > 1}}"><text class="txt-count">{{itemList.length - 1 + '/' + limit}}</text></block></view></view><view class="uploading-tip" tt:if="{{uploading}}">图片上传中···</view></view>