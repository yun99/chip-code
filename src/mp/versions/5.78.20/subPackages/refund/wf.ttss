.slider{background-image:url("data:image/png;base64,");background-position-x:10rpx;background-repeat:no-repeat;background-size:490rpx 100%;border-radius:8rpx;cursor:pointer;height:16rpx;margin:0 auto;position:relative;width:510rpx}.slider__bar{height:100%;position:relative;transition:all .2s;width:0}.slider__button{background-color:#fff;border:14rpx solid #fe8c00;border-radius:50%;box-shadow:0 5px 12px 0 rgba(254,140,0,.4);height:32rpx;width:32rpx}.slider__button-wrapper,.slider__tip-wrapper{position:absolute;right:0;top:50%;transform:translate3d(50%,-50%,0)}.slider__tip-wrapper{font-family:PingFangSC-Semibold;font-size:24rpx;height:52rpx;line-height:52rpx;text-align:center;top:-52rpx;width:110rpx}.slider__tip-wrapper .image{height:100%;left:0;position:absolute;top:0;width:100%}