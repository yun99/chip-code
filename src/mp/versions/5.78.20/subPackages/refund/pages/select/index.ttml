<wxs module="fps" src="../../../../ij.sjs" /><include src="../../../../er.ttml" /><import src="../../../../oq.ttml" /><view><view class="desc-bar"><text bindtap="showDescDialog">退款说明</text></view><view class="sku-list"><view class="sku-item" catch:tap="onSkuSelect" data-selected="{{item.selected}}" data-item-key="{{item.itemKey}}" data-index="{{index}}" tt:for="{{skuItems}}" tt:key="index"><view class="select-wrap"><image class="icon" src="{{onlySelectAll ? 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon_sku_selected_disabled.8c64353.png' : (item.selected ? 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon_sku_selected.a92f356.png' : 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon_sku_unselect.08e1358.png')}}" /></view><view class="pic-wrap"><template is="sku-item-pic" data="{{...item}}"></template></view><view class="main-wrap"><view class="name">{{item.name}}</view><view class="view-quantity">{{item.viewQuantity}}</view><view class="counter" tt:if="{{item.isCanEditQuantity && item.quantity !== 1 && item.selected}}"><yd size="small" initial-value="{{item.count}}" min-num="{{1}}" max-num="{{item.quantity}}" data-index="{{index}}" data-item-key="{{item.itemKey}}" bindchange="onSkuCountChange"></yd></view></view></view></view><view class="bottom-bar"><view class="select-wrap" catchtap="onSelectAll" data-selected="{{skuCount === skuQuantity}}"><image class="icon" src="{{onlySelectAll ? 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon_sku_selected_disabled.8c64353.png' : (skuCount === skuQuantity ? 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon_sku_selected.a92f356.png' : 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/icon_sku_unselect.08e1358.png')}}"></image><text>全选</text></view><view><button class="btn-next btn-primary {{skuCount ? '' : 'disabled'}}" disabled="{{!skuCount}}" bindtap="onNext">下一步</button></view></view></view><r id="dialog-refund-desc" custom-content-cls="dialog-refund-content" custom-header-cls="dialog-refund-header"></r><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>