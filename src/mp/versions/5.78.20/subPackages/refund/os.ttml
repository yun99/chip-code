<template name="detailProcess"><view class="detail-process-container"><view class="detail-process-item detail-process-item-show-button-{{!!isShowButton}} detail-process-item-first-{{index === 0}} detail-process-item-last-{{index === detailedStackReverseProgressNodeList.length - 1}}" tt:for="{{detailedStackReverseProgressNodeList}}" tt:if="{{index === 0 || !!showMore}}"><view class="point point-index-{{index}} point-{{item.status}}"></view><view class="detail-process-title detail-process-title-{{item.status}}"><view tt:if="{{item.afterTitle && item.afterTitle.length}}">{{item.afterTitle[0]}}<view class="text red">{{item.afterTitle[1]}}</view>{{item.afterTitle[2]}}</view><view tt:else>{{item.title}}</view></view><view class="detail-process-subtitle detail-process-subtitle-{{item.status}}" tt:if="{{item.subTitle}}"><view tt:if="{{item.afterSubTitle && item.afterSubTitle.length}}">{{item.afterSubTitle[0]}}<view class="text red">{{item.afterSubTitle[1]}}</view>{{item.afterSubTitle[2]}}</view><view tt:else>{{item.subTitle}}</view></view><view class="detail-process-time-str">{{item.timeStr}}</view></view></view></template>