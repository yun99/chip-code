.number{align-items:center;display:flex;height:60rpx;max-width:400rpx;min-width:200rpx}.number .action{background:#fff;background-size:100% 100%;color:#333;flex-basis:60rpx;flex-shrink:0;font-size:34rpx;font-weight:700;line-height:60rpx;text-align:center}.number .action.subtract{background-image:url(data:image/png;base64,)}.number .action.subtract.disabled{background-image:url(data:image/png;base64,)}.number .action.add{background-image:url(data:image/png;base64,)}.number .action.add.disabled{background-image:url(data:image/png;base64,)}.number .value{color:#121924;font-size:24px;line-height:60rpx;min-width:80rpx;text-align:center}.number.small{min-width:170rpx}.number.small .action{border-radius:22rpx;flex-basis:48rpx;font-size:32rpx;height:48rpx;line-height:38rpx;overflow:hidden;width:48rpx}.number.small .value{font-size:24rpx;line-height:42rpx}.number.large{min-width:202rpx}.number.large .action{flex-basis:52rpx;height:52rpx;width:52rpx}.number.large .value{color:#333;font-size:24rpx;width:98rpx}