<view class="sku-list {{isHorizontal ? 'horizontal' : 'vertical'}} {{size}} external-classes"><view class="item-container {{size}}"><view class="sku-wrapper {{size}}" tt:for="{{skuList}}" tt:key="skuId" tt:for-index="skuIndex"><ux schema="{{newCardSchema}}" sku="{{item}}" need-animation="{{needAnimation}}" index="{{skuIndex}}" type="{{skuType}}" canDirect="{{true}}" proxy-data="{{skuProxyData}}" bind:addToCartCompletely="onAddToCartCompletely"></ux></view><view bindtap="gotoMore" class="goto-more" tt:if="{{isHorizontal && skuList && skuList.length && skuList.length>showMoreMinNum}}"><view class="see-more {{size}} {{isShowSpecPrice ? 'high' : ''}}" tt:if="{{isShowMore}}"><block tt:if="{{size === 'mini'}}"><view class="see-more-ch">{{ENUM.seeMoreCh}}</view><view class="line"></view><view class="see-more-en">{{ENUM.seeMoreEn}}</view></block><block tt:if="{{size === 'small'}}"><view class="see-more-ch small"><view class="txt">{{ENUM.seeMoreCh2}}</view><view><image class="arrow-icon" model="scaleInFill" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/arrow_right@3x.463126.png" lazy-load="{{true}}"></image></view></view></block></view></view></view></view><ir tt:if="{{showCartIcon}}"></ir><view class="bottom-brand"></view>