<ed type="bottom" show="{{show}}" bindclose="onOk" is-full-screen="{{true}}" catchtouchmove="prevents" z-index="{{100}}" mask="{{true}}" closeOnClickMask="{{true}}" containerStyle="" maskStyle="" isShowIpxBar="{{true}}"><view class="search-screen"><scroll-view scroll-y="{{true}}" class="search-screen-area"><view class="search-screen-select-area"><view class="sku-special-screen-container" tt:if="{{skuSpecialScreenData}}"><view class="screen-title">商品特色</view><view class="screen-list"><block tt:for="{{skuSpecialScreenData && skuSpecialScreenData.radioList || []}}" tt:key="index"><view class="screen-item {{item && item.selected ? 'item-select' : ''}}" data-screen-item="{{skuSpecialScreenData.screenItem}}" data-item="{{item}}" data-type="Special" data-index="{{index}}" data-feature-name="{{item.meta.itemValue || ''}}" data-feature="商品特色" data-feature-index="{{0}}" bindtap="tagSelect"><view class="screen-item-text">{{item.meta.itemValue || ''}}</view><image class="screen-item-select" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/screen_tag_select.bd35635.png" tt:if="{{item && item.selected}}" /></view></block></view></view><view class="price-screen-container" tt:if="{{isShowPrice}}"><view class="screen-title">价格区间（元）</view><view class="screen-list"><block tt:for="{{screenData.priceItems || []}}" tt:key="index"><view class="screen-item {{priceSelectIndex === index ? 'item-select' : ''}}" data-item="{{item}}" data-index="{{index}}" data-type="Price" bindtap="tagSelect"><view class="screen-item-text">{{item.lowPrice/100}}-{{item.highPrice/100}}</view><image class="screen-item-select" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/screen_tag_select.bd35635.png" tt:if="{{priceSelectIndex === index}}" /></view></block></view><view class="price-input-container"><input class="input-label {{inputLowPriceFocus ? 'input-focus' : ''}} {{lowPrice || lowPrice===0 ? 'has-value' : ''}}" value="{{lowPrice==='' ? '' : lowPrice/100}}" name="lowPrice" maxlength="8" type="number" data-input-focus="lowPrice" bindfocus="inputGetFocus" bindblur="inputLoseFocus" bindinput="onLowHighPriceInput" placeholder="最低价" /><view class="line"></view><input class="input-label {{inputHighPriceFocus ? 'input-focus' : ''}} {{highPrice || highPrice===0 ? 'has-value' : ''}}" value="{{highPrice==='' ? '' : highPrice/100}}" name="highPrice" maxlength="8" type="number" data-input-focus="highPrice" bindfocus="inputGetFocus" bindblur="inputLoseFocus" bindinput="onLowHighPriceInput" placeholder="最高价" /></view></view><view class="sku-property-screen-container" tt:for="{{skuPropertyScreenData}}" tt:key="siftIndex" tt:for-item="siftItem" tt:for-index="siftIndex"><view class="screen-title" tt:if="{{siftItem.radioList.length}}"><view class="screen-title-text">{{siftItem.screenItem.itemValue}}</view><view class="screen-title-fold" data-screen-item="{{siftItem.screenItem}}" data-item="{{radioItem}}" data-type="Property" bindtap="togglePanel" tt:if="{{siftItem.radioList.length > referenceFoldNum}}"><image class="image" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/down_arrow.20f0225.png" tt:if="{{siftItem.isFold}}"></image><image class="image" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/up_arrow.5646827.png" tt:else></image></view></view><view class="screen-list {{siftItem.isFold ? 'screen-list-fold' : ''}}" tt:if="{{siftItem.radioList.length}}"><block tt:for="{{siftItem.radioList || []}}" tt:key="index" tt:for-item="radioItem"><view class="screen-item {{radioItem && radioItem.selected ? 'item-select' : ''}}" data-screen-item="{{siftItem.screenItem}}" data-item="{{radioItem}}" data-index="{{index}}" data-feature-name="{{radioItem.meta.itemValue || ''}}" data-feature="{{siftItem.screenItem.itemValue}}" data-feature-index="{{siftIndex}}" data-type="Property" bindtap="tagSelect"><view class="screen-item-text">{{radioItem.meta.itemValue || ''}}</view><image class="screen-item-select" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/screen_tag_select.bd35635.png" tt:if="{{radioItem && radioItem.selected}}" /></view></block></view></view></view></scroll-view><view class="selected-panel-area {{isIpx ? 'selected-panel-ipx' : ''}}"><view class="reset-btn" data-is-btn="{{true}}" bindtap="reset">重置</view><view class="confirm-btn" bindtap="onOk" type="warn">确定</view></view></view></ed>