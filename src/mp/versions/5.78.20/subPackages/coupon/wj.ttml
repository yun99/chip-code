<view class="sku-container {{isLastItem ? 'sku-container-border-none' : ''}} {{totalItemCount === 1 ? 'sku-container-padding-none' : ''}}"><view class="condition" tt:if="{{totalItemCount !== 1}}">{{skuItem.promotionCurRule}}</view><view class="redemption-select-wrapper {{skuItem.valid ? '' : 'redemption-select-wrapper-mask'}}" tt:for="{{skuItem.exchangeItems}}" tt:for-item="skuExchangeItem" tt:key="index"><view class="redemption-select-image" bindtap="onSelect" data-sku-id="{{skuExchangeItem.skuId}}" data-selected="{{skuExchangeItem.selected}}" data-can-select="{{skuExchangeItem.entryType === 1 || skuExchangeItem.entryType === 2 || skuExchangeItem.entryType === 3}}" tt:if="{{skuExchangeItem.entryType === 1 || skuExchangeItem.entryType === 2 || skuExchangeItem.entryType === 3}}"><wq src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/check.bcc4917.png" webp-conf="{{({width: '40rpx', height: '40rpx'})}}" tt:if="{{skuExchangeItem.selected}}"></wq><view class="redemption-select-circle" tt:else></view></view><view class="redemption-select-image" bindtap="toDetail" data-poi-id="{{skuExchangeItem.poiId}}" data-poi-id-str="{{skuExchangeItem.poiIdStr}}" data-sku-id="{{skuExchangeItem.skuId}}" tt:elif="{{skuExchangeItem.entryType === 11}}"><wq src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/oval.6e74521.png" webp-conf="{{({width: '40rpx', height: '40rpx'})}}"></wq></view><view class="redemption-info" bindtap="toDetail" data-poi-id="{{skuExchangeItem.poiId}}" data-poi-id-str="{{skuExchangeItem.poiIdStr}}" data-sku-id="{{skuExchangeItem.skuId}}"><view class="redemption__image-wrap"><wq webp-conf="{{({width: '145rpx', height: '145rpx'})}}" custom-class="redemption__image" src="{{skuExchangeItem.pic}}" /><block tt:if="{{skuExchangeItem.entryType === 11}}"><view class="redemption__image-mask" /><view class="redemption__image-tag">抢光了</view></block></view><view class="redemption-detail"><view class="redemption-detail-title">{{skuExchangeItem.title}}</view><view class="redemption-detail-spec">{{skuExchangeItem.spec}}</view><view class="redemption-detail-spare-info"><view class="redemption-detail-spare-info-price">¥{{skuExchangeItem.sellPriceUnitInfoV3.sellUnitViewPrice / 100}}</view><view class="redemption-detail-spare-info-unit" tt:if="{{skuExchangeItem.sellPriceUnitInfoV3.sellUnitViewName}}">/{{skuExchangeItem.sellPriceUnitInfoV3.sellUnitViewName}}</view><view class="redemption-detail-spare-info-line-price">¥{{skuExchangeItem.sellPriceUnitInfoV3.sellUnitViewLinePrice / 100}}</view><view class="redemption-detail-spare-info-num">x{{skuExchangeItem.sellPriceUnitInfoV3.viewQuantity}}</view></view></view></view></view></view>