<tf show="{{show}}" type="bottom" show-mask="{{show}}" bind:maskclick="handleMaskClick"><view class="gift-container"><view class="title">{{isRedemption ? '换购商品' : '查看赠品'}}<view class="close-wrapper" bindtap="handleMaskClick"><wq webp-conf="{{({width: '25rpx', height: '25rpx'})}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/f29ff85f-6c00-444c-8e50-ac686a18967a_1543200152279.afa5264.png"></wq></view></view><view class="content-wrapper" catchtouchmove="true"><scroll-view scroll-y class="content"><block tt:if="{{isRedemption}}"><iu sku-item="{{item}}" isLastItem="{{index == giftSkuList.length - 1}}" promotionId="{{promotionId}}" hasExchangeCount="{{hasExchangeCount}}" totalExchangeCount="{{totalExchangeCount}}" bind:changeCount="changeCount" bind:skuSelect="skuSelect" index="{{index}}" totalItemCount="{{giftSkuList.length}}" tt:for="{{giftSkuList}}" tt:key="skuId"></iu></block><block tt:else><iy sku-item="{{item}}" tt:for="{{giftSkuList}}" tt:key="skuId"></iy></block></scroll-view><view class="close"><block tt:if="{{isRedemption && !unValid}}"><view class="exchange-count">已换购{{hasExchangeCount}}/{{totalExchangeCount}}件</view><view class="confirm" bindtap="confirmRedemption">确定</view></block><view class="close-button" bindtap="handleMaskClick" tt:else>关闭</view></view></view></view></tf>