.sku-container{border-bottom:1rpx solid #eee;padding-top:20rpx}.sku-container .redemption-select-wrapper{align-items:center;display:flex;flex-direction:row;flex-wrap:nowrap;overflow:hidden;padding:30rpx 0 0 30rpx}.sku-container .redemption-select-wrapper .redemption-select-image{align-items:center;display:flex;flex-direction:column;height:60rpx;justify-content:center;margin-bottom:30rpx;margin-right:10rpx;width:54rpx}.sku-container .redemption-select-wrapper .redemption-select-image .redemption-select-circle{border:2rpx solid #cfcfcf;border-radius:50%;box-sizing:border-box;height:40rpx;opacity:.4;width:40rpx}.sku-container .redemption-select-wrapper .redemption-info{border-bottom:1rpx solid #eee;display:flex;flex:1;padding:0 30rpx 30rpx 10rpx}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail{display:flex;flex:1;flex-direction:column}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail-title{color:#333;font-size:28rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail-spec{color:#999;font-size:24rpx}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail-spare-info{align-items:center;color:#999;display:flex;font-size:24rpx;margin-top:auto}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail-spare-info-price{color:#ff1929;font-size:32rpx}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail-spare-info-line-price{flex:1;margin-left:8rpx;text-decoration:line-through}.sku-container .redemption-select-wrapper .redemption-info .redemption-detail-spare-info-unit{color:#999;font-size:24rpx}.sku-container .redemption-select-wrapper-mask{opacity:.6}.sku-container .redemption-select-wrapper:last-child .redemption-info{border-bottom:0}.sku-container .condition{color:#333;font-size:26rpx;padding:0 30rpx}.sku-container .condition.invalid{color:#ff1929}.sku-container-border-none{border-bottom:0}.sku-container-padding-none{padding-top:0}.redemption__image-wrap{margin-right:20rpx;position:relative}.redemption__image{display:block;height:145rpx;width:145rpx}.redemption__image-mask{background-color:hsla(0,0%,100%,.3);bottom:0;left:0;position:absolute;right:0;top:0}.redemption__image-tag{background-color:rgba(0,0,0,.44);border-radius:42rpx;color:#fff;font-size:24rpx;left:50%;line-height:42rpx;position:absolute;text-align:center;top:50%;transform:translate(-50%,-50%);width:100rpx}