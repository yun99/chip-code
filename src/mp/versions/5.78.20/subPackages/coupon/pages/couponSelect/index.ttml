<wxs module="fps" src="../../../../ij.sjs" /><include src="../../../../er.ttml" /><view class="index-page"><view tt:if="{{canUseCouponList.length || inCouponList.length}}"><view bindtap="changeSelected" data-id="{{item.id}}" tt:for="{{canUseCouponList}}" tt:key="{{item.couponId}}"><es coupon="{{item}}" pageType="{{pageType}}"></es></view><view class="invalid" tt:if="{{inCouponList.length}}"><view class="line"></view><view class="dot"></view><text>不可用优惠券({{inCouponList.length}}张)</text><view class="dot right-dot"></view><view class="line right-line"></view></view><view tt:for="{{inCouponList}}" tt:key="index"><es coupon="{{item}}" pageType="{{pageType}}"><view slot="desc-static" class="coupon-invalid" tt:if="{{!item.valid}}"><view class="invalid-title">{{item.limitLabel}}</view><view class="invalid-reason" tt:if="{{item.limit}}"><text>{{item.limit}}</text><block tt:if="{{item.limitDetail}}">（<text>{{item.limitDetail}}</text><text class="invalid-reason-btn" bindtap="showLimitDetail" data-index="{{index}}" tt:if="{{item.limitDetailLabel}}">{{item.limitDetailLabel}}</text>）</block></view></view></es></view><view class="in-coupon-bottom"></view></view><view class="no-coupon" bindtap="goBack">不使用优惠券</view></view><ed type="bottom" show="{{showModal}}" bindclose="closeModal" is-full-screen="{{true}}" catchtouchmove="true"><view class="invalid-pop-main"><view class="invalid-pop-title">{{limitDetailTitle}}</view><scroll-view class="invalid-pop-content" scroll-y="{{true}}"><view class="invalid-pop-item {{index === (skuBlack.length - 1) ? 'no-border': ''}}" tt:for="{{skuBlack}}" tt:key="index"><view class="invalid-pop-l"><wq webp-conf="{{({width: '119rpx', height: '112rpx'})}}" src="{{item.pic}}"></wq></view><view class="invalid-pop-r"><view class="slogens-title">{{item.skuName}}</view><view class="invalid-pop-detail"><block tt:if="{{item.promotionPrice}}"><text class="price-now">￥{{item.promotionPrice}}</text><text class="price-origin">￥{{item.sellPrice}}</text></block><text class="price-now" tt:else>￥{{item.sellPrice}}</text></view></view></view></scroll-view><view class="invalid-pop-bottom" bindtap="closeModal">确定</view></view></ed><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>