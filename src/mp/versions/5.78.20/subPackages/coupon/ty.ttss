.gift-container{background:#fff;border-radius:24rpx 24rpx 0 0;box-sizing:border-box;width:750rpx}.gift-container .title{align-items:center;border-bottom:1rpx solid #e1e1e1;color:#121924;display:flex;font-family:PingFangSC-Medium;font-size:36rpx;font-weight:700;height:100rpx;justify-content:center;letter-spacing:-.77px;position:relative;width:100%}.gift-container .title .close-wrapper{align-items:center;display:inline-flex;height:100%;justify-content:center;position:absolute;right:0;top:50%;transform:translateY(-50%);width:107rpx}.gift-container .content-wrapper .content{max-height:730rpx}.gift-container .content-wrapper .close{align-items:center;border-top:1rpx solid #eee;display:flex;height:fit-content;justify-content:center;padding:30rpx;text-align:center}.gift-container .content-wrapper .close-button{background:#ffd100;border-radius:100rpx;color:#121924;font-family:PingFangSC-Medium;font-size:34rpx;font-weight:700;height:90rpx;line-height:90rpx;width:100%}.gift-container .content-wrapper .close .exchange-count{color:#999;flex:1;font-size:26rpx}.gift-container .content-wrapper .close .confirm{background:#ffd100;border-radius:100px;color:#121924;font-family:PingFangSC-Medium;font-size:34rpx;font-weight:700;height:90rpx;line-height:90rpx;text-align:center;width:400rpx}