<wxs module="fps" src="../../../../ij.sjs" /><include src="../../../../er.ttml" /><wd content-style="background-color: #FFF;" tt:if="{{!isNotSupportNavigationStyle}}"><tm title="{{title}}" bindback="goBack"></tm></wd><block tt:if="{{receiverInfoList.length < 1 && notInRangeAddressList < 1}}"><view class="nobody-list"><view class="nobody-image"><image class="image" src="https://p0.meituan.net/mallimages/a0c4ee538abd4cb76897e215cf8a1c9a28743.png" /></view><view class="nobody-text"><view class="text-title">{{nobodyTitle}}</view><view class="text-subTitle">{{nobodySubTitle}}</view></view><view class="add-person-btn" bindtap="addReceiverBtnClick">{{nobodyBtn}}</view></view></block><block tt:else><view class="list-wrapper {{isIpx ? 'list-wrapper-isIpx' : ''}}"><yq receiverInfoList="{{receiverInfoList}}" pageFrom="{{pageFrom}}" selectID="{{selectID}}" addressId="{{addressId}}" isIpx="{{isIpx}}" bind:resetClick="resetClick" bind:changeConsignee="changeConsignee" /><yq receiverInfoList="{{notInRangeAddressList}}" pageFrom="{{pageFrom}}" selectID="{{selectID}}" addressId="{{addressId}}" isIpx="{{isIpx}}" is-not-in-range="{{true}}" bind:resetClick="resetClick" bind:changeConsignee="changeConsignee" /></view><view class="add-person {{isIpx ? 'add-person-isIpx' : ''}}"><block tt:if="{{pageFrom == 'deliveryAddress'}}"><view class="add-person-btn" bindtap="addReceiverBtnClick">添加收货地址</view></block><block tt:else><view class="add-person-btn" bindtap="addReceiverBtnClick">添加提货人</view></block></view></block><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>