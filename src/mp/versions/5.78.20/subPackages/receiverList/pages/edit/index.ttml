<wxs module="fps" src="../../../../ij.sjs" /><include src="../../../../er.ttml" /><import src="../../ou.ttml" /><view class="navWrapper"><wd contentStyle="background:#fff;" tt:if="{{!isNotSupportNavigationStyle}}"><tm title="{{title}}" bindback="handleBack" /></wd></view><view class="edit-page-container"><block tt:if="{{pageFrom == 'deliveryAddress'}}"><view class="item-module"><tn type="text" label="收货人" pageFrom="{{pageFrom}}" placeholder="请输入收货人名称" edit-type="pickerName" value="{{pickerName}}" max-length="{{20}}" bindinput="handleInput" bindclear="handleClear" bindfocus="handleFocus" /><tn type="number" label="手机号" pageFrom="{{pageFrom}}" placeholder="请输入收货人的手机号" edit-type="pickerPhone" value="{{pickerPhone}}" max-length="{{11}}" bindinput="handleInput" bindclear="handleClear" bindfocus="handleFocus" /><tn type="text" label="收货地址" pageFrom="{{pageFrom}}" placeholder="请选择收货地址" value="{{chooseAddress}}" edit-type="arrow" catchtap="handleArrowClick" /><tn type="text" label="门牌号" pageFrom="{{pageFrom}}" placeholder="详细地址，例如：1号楼2单元101" edit-type="houseNumber" value="{{houseNumber}}" max-length="{{50}}" bindinput="handleInput" bindclear="handleClear" bindfocus="handleFocus" /></view></block><block tt:else><view class="item-module"><tn type="text" label="提货人" no-line="{{true}}" required="{{true}}" placeholder="请输入姓名" edit-type="pickerName" value="{{pickerName}}" max-length="{{16}}" bindinput="handleInput" bindclear="handleClear" bindfocus="handleFocus" /><tn type="number" label="手机号" required="{{true}}" placeholder="请输入手机号" edit-type="pickerPhone" value="{{pickerPhone}}" max-length="{{11}}" bindinput="handleInput" bindclear="handleClear" bindfocus="handleFocus" /></view><view class="item-module"><tn type="text" label="标签" placeholder="请输入标签，最多10个字" no-line="{{true}}" edit-type="label" value="{{label}}" max-length="{{10}}" bindinput="handleInput" bindclear="handleClear" bindfocus="handleFocus" /><view class="tag-wrapper"><template is="tags" data="{{tagList, selectedTag}}" /></view><view class="item type-item"><view class="left-content"><view class="label">设为默认提货人</view><view class="tips">每次下单会默认使用该提货人</view></view><view class="switch-wrapper"><switch checked="{{!!pickerType}}" color="#FFD100" bindchange="handleTypeChange" /></view></view></view></block><view class="delete-btn" catchtap="handleDelete" tt:if="{{showDeleteBtn}}"><block tt:if="{{pageFrom == 'deliveryAddress'}}">删除此收货地址</block><block tt:else>删除此提货人</block></view><view class="bottom {{isIpx ? 'ipx-bottom' : ''}}"><view class="btn {{btnDisabled ? 'btn-disabled' : ''}}" catchtap="handleEditUserInfo">保存</view></view><block tt:if="{{pageFrom == 'deliveryAddress'}}"><r show="{{showDeleteDialog}}" content="确定要删除此收货地址吗?" cancel-text="取消" ok-text="删除" bindok="deleteAddress" bindcancel="handleDeleteDialogCancel" /></block><block tt:else><r show="{{showDeleteDialog}}" content="确定要删除此提货人吗?" cancel-text="取消" ok-text="删除" bindok="deletePicker" bindcancel="handleDeleteDialogCancel" /></block><r show="{{showSaveDialog}}" content="是否保存本次编辑的信息？" cancel-text="不保存" ok-text="保存" bindok="handleEditUserInfo" bindcancel="handleSaveDialogCancel" /></view><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>