umn;padding-right:30rpx}.preson-msg-detail-delivery-content-line{display:flex;flex-direction:row}.preson-msg-detail-delivery-left{display:flex;flex:1;flex-direction:row}.person-msg-detail-delivery-right{display:flex;right:0rpx}.person-msg-reset-delivery{align-self:center;color:#7f7f7f;font-size:28rpx;padding-left:30rpx;padding-right:30rpx}.person-msg-reset-delivery-div{align-self:center;background-color:#d6d8de;height:32rpx;width:2rpx}.person-msg-detail-delivery-name{margin-right:18rpx;max-width:288rpx}.person-msg-detail-delivery-name,.person-msg-detail-delivery-phone{color:#191919;font-family:PingFangSC-Medium;font-size:32rpx;font-weight:500;line-height:44rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.preson-msg-detail-delivery-address{color:#7f7f7f;font-family:PingFangSC-Regular;font-size:28rpx;font-weight:400;padding-top:6rpx}.person-msg-detail-delivery-container-div{background-color:#eee;height:1rpx;margin-left:62rpx}.person-name{margin-right:55rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;width:224rpx}.person-msg-reset{border-left:2rpx solid #d6d8de;color:#7f7f7f;font-size:28rpx;line-height:40rpx;padding-left:30rpx}