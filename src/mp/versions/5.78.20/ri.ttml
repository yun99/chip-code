<wxs module="tools" src="./jr.sjs"></wxs><view class="container" style="{{wrapperStyle}}" tt:if="{{((hasTabBar || needHide) && totalNum > 0) || (!hasTabBar && !needHide)}}"><view class="main-body"><view class="left-area"><view class="bottom-entry entry-icon home" bindtap="goOrderPage" tt:if="{{isShareLandingPage}}"><view class="icon-wrap"><wq webp-conf="{{({width:'44rpx', height:'44rpx'})}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/new_add_cart_bar_order_thin.de7d1026.png" /></view><view class="bottom-icon-text g-text-7 g-color-7">订单</view></view><view class="entry-icon share" tt:if="{{showShareIcon}}"><button class="bottom-entry share-btn" data-share-position="12" data-share-button-object="{{shareButtonObject}}" open-type="{{isAutoShare ? 'share' : ''}}" bindtap="openShare"><view class="icon-wrap"><wq webp-conf="{{({width:'44rpx', height:'44rpx'})}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/new_add_cart_bar_share.10b11028.png" /></view><view class="bottom-icon-text g-text-7 g-color-7">分享</view></button></view><view class="bottom-entry cart-b" bindtap="gotoCartLanding" tt:if="{{hasTabBar}}"><view class="icon-wrap-b"><view class="cart-num g-text-5 g-font-bold g-color-dark g-color-1 {{tools.getCartClass(totalNum)}}" tt:if="{{totalNum > 0}}">{{totalNum}}</view><wq webp-conf="{{({width:'48rpx', height:'48rpx'})}}" id="cart-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/new_add_cart_bar_cart_v2.5d8a1021.png" /></view></view><view class="bottom-entry cart" bindtap="gotoCartLanding" tt:else><view class="icon-wrap"><view class="cart-num g-text-5 g-font-bold g-color-dark g-color-1 {{tools.getCartClass(totalNum)}}" tt:if="{{totalNum > 0}}">{{totalNum}}</view><wq webp-conf="{{({width:'44rpx', height:'44rpx'})}}" id="cart-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/new_add_cart_bar_cart.81b41022.png" mode="scaleToFill" /></view><view class="bottom-icon-text g-text-7 g-color-7">购物车</view></view></view><view class="right-area"><view class="button-wrap" bind:tap="goSubmitOrderPageThrottle"><block tt:if="{{isReducedAmountStyleABGrey}}"><block tt:if="{{reducedAmount}}"><view class="g-price g-price-4 special-amount-price" tt:if="{{hasTabBar}}">已节省{{reducedAmount / 100}}元</view><view class="subpage-price-wrap" tt:else><view class="g-price g-price-2 special-price"><view class="g-price-unit special-price-unit">¥</view>{{totalPrice / 100}}</view><view class="g-price g-price-5 special-amount-price">已节省¥{{reducedAmount / 100}}</view></view><view class="divide-line"></view></block><view class="button-right-wrap"><view class="g-price g-price-1 special-price" tt:if="{{hasTabBar || !reducedAmount}}"><view class="g-price-unit special-price-unit">¥</view>{{totalPrice / 100}}</view><z no-data="{{true}}" bindmvReport="onMvReport"><view class="g-text-1 g-color-3 submit-text">去支付</view></z><wq class="arrow-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/new_add_cart_bar_arrow.c0281020.png" mode="scaleToFill" webp-conf="{{({width:'24rpx', height:'24rpx'})}}" isBlockImg="{{true}}" /></view></block><block tt:else><view class="g-price g-price-1 special-price"><view class="g-price-unit special-price-unit">¥</view>{{totalPrice / 100}}</view><z no-data="{{true}}" bindmvReport="onMvReport"><view class="g-text-1 g-color-3 submit-text">去支付</view></z><wq class="arrow-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/new_add_cart_bar_arrow.c0281020.png" mode="scaleToFill" webp-conf="{{({width:'24rpx', height:'24rpx'})}}" isBlockImg="{{true}}" /></block></view></view></view><view class="ipx-placeholder" tt:if="{{!hasTabBar && adaptSafeArea && isIpx}}"></view></view>