<view class="card card-background {{nodeId === currentTouchNodeId && skuItems.skuId === skuId ? 'offset' : ''}}" data-sku-id="{{skuItems.skuId}}" data-del-type="{{canSelect ? 'delete' : 'giftDelete'}}" catch:longpress="onSkuCountChange" bind:tap="toSkuDetail"><view class="card-mask" tt:if="{{!canSelect && (!skuItems.giftState.achieveGiftCondition || skuItems.giftState.type === 11)}}"></view><z lx-params="{{({skuId: skuItems.skuId})}}" bindmvReport="onCardMvReport"><wq class="flex-align-center card-select" webp-conf="{{({width: '44rpx', height: '44rpx' })}}" src="{{skuItems.selected ? 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-popup-selected.aaa1913.png' : 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-popup-unselect.f914914.png'}}" data-sku-id="{{skuItems.skuId}}" data-selected="{{skuItems.selected}}" catch:tap="onSkuSelectClick" styleStr="{{canSelect ? '' : 'visibility:hidden'}}" /></z><view class="card-box cart-28-{{fontRate}}"><view class="card-box-content"><wq class="card-box-content-image" src="{{skuItems.pic}}" webp-conf="{{({width: '200rpx', height: '200rpx' })}}" styleStr="display:block;border-radius:12rpx;" /><view class="card-box-content-message"><view><view class="card-box-content-message_title"><view class="card-box-content-message_title-imageWrap"><image mode="heightFix" class="card-box-content-message_title-image" src="{{skuItems.activityTag}}" tt:show="{{skuItems.activityTag}}" /></view><text class="card-box-content-message_title-text">{{skuItems.title}}</text></view><view class="card-box-content_label-box cart-24-{{fontRate}}"><view class="label walk-return-money" tt:if="{{canSelect && skuItems.walkReturnMoneyInfo.walkIconUrl && skuItems.walkReturnMoneyInfo.walkText}}"><wq class="walk-return-money_icon" webp-conf="{{({width: '30rpx', height: '30rpx' })}}" src="{{skuItems.walkReturnMoneyInfo.walkIconUrl}}" /><z customStyle="display:inline" bindmvReport="onWalkReturnMoneyInfoMvReport" noData="{{true}}">{{skuItems.walkReturnMoneyInfo.walkText}}</z></view><view class="label {{skuItems.walkReturnMoneyInfo.walkIconUrl && skuItems.walkReturnMoneyInfo.walkText ? 'sell-info-label' : ''}}" tt:if="{{(canSelect && skuItems.sellInfoLabel) || (!canSelect && skuItems.cartItemStyle && skuItems.cartItemStyle.titleSideLabel && skuItems.cartItemStyle.titleSideLabel.desc)}}">{{canSelect ? skuItems.sellInfoLabel : skuItems.cartItemStyle.titleSideLabel.desc}}</view><view class="label {{(canSelect && skuItems.walkReturnMoneyInfo.walkIconUrl && skuItems.walkReturnMoneyInfo.walkText) || (canSelect && skuItems.sellInfoLabel) || (!canSelect && skuItems.cartItemStyle && skuItems.cartItemStyle.titleSideLabel && skuItems.cartItemStyle.titleSideLabel.desc) ? 'self-lifting-label' : ''}}" tt:if="{{(canSelect && skuItems.selfLiftingLabel) || (!canSelect && skuItems.cartItemStyle && skuItems.cartItemStyle.selfLiftingLable && skuItems.cartItemStyle.selfLiftingLable.text)}}">{{canSelect ? skuItems.selfLiftingLabel : skuItems.cartItemStyle.selfLiftingLable.text}}</view></view></view><view class="bottom-part"><view class="flex-align-center promotion-label-box" tt:if="{{skuItems.singleSkuPromotionLabel || skuItems.depreciateLabel || skuItems.singleSkuPromotionUrl}}"><wq mode="heightFix" class="" src="{{skuItems.singleSkuPromotionUrl}}" webp-conf="{{({height: '40rpx'})}}" styleStr="display:block;margin-bottom: -6rpx;" tt:if="{{skuItems.singleSkuPromotionUrl}}" /><view class="promotion-label cart-24-{{fontRate}}" tt:elif="{{skuItems.singleSkuPromotionLabel}}">{{skuItems.singleSkuPromotionLabel}}</view><view class="coupon-label {{skuItems.singleSkuPromotionLabel ? 'red-circle' : ''}} cart-24-{{fontRate}}" tt:if="{{skuItems.depreciateLabel}}">{{skuItems.depreciateLabel}}</view></view><view class="original-price cart-26-{{fontRate}}" tt:if="{{skuItems.referencePrice}}">¥<view class="original-price_num">{{skuItems.referencePrice / 100}}</view></view><view class="price {{skuItems.estimatedHandPriceText.length === 2 ? 'estimated-black' : ''}} {{skuItems.memberLabel ? 'brown' : ''}}"><view class="price-symbol cart-26-{{fontRate}}">¥</view><view class="price-price cart-36-{{fontRate}}">{{canSelect ? skuItems.sellerPrice / 100 : skuItems.sellPriceUnitInfoV3.sellUnitViewPrice / 100}}</view><view class="flex-align-center price-member" tt:if="{{skuItems.memberLabel}}"><view class="price-member_label cart-22-{{fontRate}}">{{skuItems.memberLabel}}</view></view><og count="{{skuItems.count}}" change-step="{{1}}" data-sku-id="{{skuItems.skuId}}" bind:change="onSkuCountChange" tt:if="{{canSelect}}" /><view class="gift-count cart-30-{{fontRate}}" tt:else>x{{skuItems.giftState.giftCount}}</view></view></view></view></view><view class="flex-align-center price-estimated cart-28-{{fontRate}}" tt:if="{{skuItems.estimatedHandPriceText.length === 2}}">{{skuItems.estimatedHandPriceText[0]}}<view class="price-estimated-symbol">¥</view>{{skuItems.estimatedHandPriceText[1]}}</view></view><view class="flex-align-center sku-delete-sku-btn cart-26-{{fontRate}}" catch:tap="onSkuCountChange" data-del-type="{{canSelect ? 'delete' : 'giftDelete'}}" data-sku-id="{{skuItems.skuId}}">删除</view><view class="flex-align-center text-promotion-limit {{skuItems.estimatedHandPriceText.length === 2 ? '' : 'text-promotion-limit-top'}}" tt:if="{{skuItems.promotionLimitText}}"><wq webp-conf="{{({width: '32rpx', height: '8rpx' })}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-promotionLimitText-triangle.2ef1915.png" styleStr="display:block;margin-right:72rpx;" /><z lx-params="{{({skuId: skuItems.skuId, message: skuItems.promotionLimitText})}}" bindmvReport="onPromotionLimitMvReport"><view class="line-ellipsis_2 promotion-limit-content cart-26-{{fontRate}}">{{skuItems.promotionLimitText}}</view></z></view></view>