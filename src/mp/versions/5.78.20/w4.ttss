@import "./jn.ttss";.root{flex-direction:column;height:100%;position:relative}.card{margin-top:18rpx}.recommend-card{padding-top:0}.rec-title{background:#fff;font-size:32rpx;height:42rpx;justify-content:center;margin-top:58rpx;position:relative;z-index:100}.rec-title_line{border-bottom:2rpx solid hsla(215,6%,63%,.3);margin:0 24rpx;width:50rpx}.rec-title_unempty{margin-top:18rpx;padding-top:18rpx;transform:unset}.guess-like-loading{box-sizing:border-box;height:132rpx;justify-content:center;padding:6rpx 0 30rpx}.guess-like-loading__icon{animation-duration:1s;animation-iteration-count:infinite;animation-name:guessLoadingAnimation;animation-timing-function:linear;height:30rpx;width:30rpx}.guess-like-loading__text{color:#b5b5b5;font-size:22rpx;margin-left:6rpx}.already-bottom{color:#95989e;font-size:26rpx;margin:46rpx;text-align:center}@keyframes guessLoadingAnimation{0%{transform:rotate(0deg)}to{transform:rotate(1turn)}}.unfill-style{background:#fafafa}.fill-style{margin-top:50%;transform:translateY(-50%)}.gradient{height:100rpx;left:0;position:absolute;top:0;width:100%}.gradient-head{background-image:linear-gradient(180deg,#fafafa,#f0f1f2)}.gradient-bottom{background-image:linear-gradient(180deg,#f0f1f2,#fff);top:-100rpx;z-index:-10}.gradient-bottom_empty{background-image:linear-gradient(180deg,#fafafa,#fff)}.no-data__wrapper{padding-top:120rpx}