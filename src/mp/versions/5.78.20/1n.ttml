<wxs module="skuBus" src="./ji.sjs"></wxs><template name="sku-img"><block tt:if="{{rendType === 'array' && skuBus.getDataByPath(sku, dataPath.default)}}"><block tt:for="{{skuBus.getDataByPath(sku, dataPath.default)|| []}}" tt:key="index"><block tt:if="{{!(options.maxLen && index >= options.maxLen )}}"><view class="bg-img-wrap {{innerClass.itemWrap}}" style="background-image: url({{item}})" tt:if="{{options.isbgImg}}"><image src="{{item}}" mode="{{options.mode}}" class="opacity_0 {{innerClass.item}}" lazy-load="{{true}}" /></view><wq webp-conf="{{(options.webpConf || { isAuto: true })}}" isBlockImg="{{true}}" src="{{item}}" mode="{{options.mode}}" class="{{options.isInline ? 'inline-block' : ''}} {{innerClass.item}}" tt:else /></block></block></block><block tt:elif="{{skuBus.getDataByPath(sku, dataPath.default)}}"><wq class="{{options.isInline ? 'inline-block' : ''}} {{classStr}}" webp-conf="{{(options.webpConf || { isAuto: true })}}" isBlockImg="{{true}}" bindload="onSkuImgLoad" data-bustype="{{options.bustype}}" mode="{{options.mode}}" src="{{skuBus.getDataByPath(sku, dataPath.priority) || skuBus.getDataByPath(sku, dataPath.default)}}" /></block></template>