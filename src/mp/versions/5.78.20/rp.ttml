<view class="bottom-brand-container" bindtap="jumpDebug" style="background:{{options.backgroundColor}};padding-top:{{options.paddingtop}}" tt:if="{{!options.isHide || options.isShow}}"><view class="brand-img"><wq webp-conf="{{({width:'600rpx', height:'60rpx'})}}" src="{{brandPicUrl}}" tt:if="{{brandPicUrl && !options.forceShowText}}" /><view class="no-network" tt:else><view class="line" style="background:{{options.textColor}};"></view><view class="text" style="color:{{options.textColor}};">已经到底啦</view><view class="line" style="background:{{options.textColor}};"></view></view><view class="app-version">当前版本: v{{appVersion}}</view></view><view class="brand-info" tt:if="{{propagandaContextV2.length && !options.forceShowText}}"><view class="info-item {{index === 1 ? 'middle' : ''}}" tt:for="{{propagandaContextV2}}" tt:key="index"><image class="info-item-icon" src="{{item.iconUrl || 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/brand_info_icon_grey.74a766.png'}}" mode="aspectFit" /><text class="info-item-text">{{item.title && item.title.text}}</text></view></view><view style="height:{{options.bottomHeight}}rpx;"></view></view><tf show="{{debugPasswordModal}}" show-mask="true" type="center"><view class="bitc-dialog--container custom-container-cls {{title ? 'bitc-dialog--container-title' : ''}}"><view class="bitc-dialog__header custom-header-cls">调试密码</view><view class="bitc-dialog__content {{title ? 'bitc-dialog__content--title' : ''}} {{contentType === 'list' ? 'bitc-dialog__content--list' : ''}} custom-content-cls"><input class="input" password type="number" bindinput="bindKeyInput" /></view><view class="bitc-dialog__footer {{buttonsShowVertical ? 'bitc-dialog__footer--vertical' : 'bitc-dialog__footer--horizon'}}"><block tt:for="{{[{text:'确认', type:'confirm', color: '#3CC51F'},{text:'取消', type:'cancel', color: '#666'}]}}" tt:key="index"><tg class="bitc-dialog__button" custom-class="{{index === 0 ? 'bitc-dialog__button-inside--first' : 'bitc-dialog__button-inside'}}" data-type="{{item.type}}" open-type="{{item.openType}}" bindtap="handleButtonClick" type="large"><view style="color: {{item.color || '#333'}}">{{item.text}}</view></tg></block></view></view></tf>