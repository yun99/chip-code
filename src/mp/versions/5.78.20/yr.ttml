<tf show="{{show}}" type="center" z-index="{{zIndex}}" bind:maskclick="handleMaskClick" isCatchTouchMove="{{isCatchTouchMove}}" tt:if="{{show}}"><view class="bitc-dialog--container custom-container-cls {{title ? 'bitc-dialog--container-title' : ''}}"><view class="bitc-dialog__header custom-header-cls" tt:if="{{title}}">{{title}}</view><view class="bitc-dialog__content {{title ? 'bitc-dialog__content--title' : ''}} {{contentType === 'list' ? 'bitc-dialog__content--list' : ''}} custom-content-cls" style="{{customContentStyle}}"><text>{{message}}</text></view><view class="bitc-dialog__footer {{buttonsShowVertical ? 'bitc-dialog__footer--vertical' : 'bitc-dialog__footer--horizon'}}"><block tt:for="{{buttons}}" tt:key="index"><tg class="bitc-dialog__button {{item.type === 'cancel' ? 'g-font-normal' : 'g-font-bold'}}" custom-class="{{buttons.length === 1 ? '' : 'bitc-dialog__button-' + item.type}} {{buttons.length === 1 ? 'bitc-dialog__button_only-' + item.type : index === 0 ? 'bitc-dialog__button-inside-common bitc-dialog__button-inside--first' : 'bitc-dialog__button-inside-common bitc-dialog__button-inside'}}" data-type="{{item.type}}" open-type="{{item.openType}}" no-after-border="{{true}}" bind:btnclick="handleButtonClick" type="large"><view>{{item.text}}</view></tg></block></view></view></tf>