<ob id="ext-poi-modal" show="{{show}}" use-slot="{{true}}" can-close="{{false}}" z-index="{{1010}}" title="{{title}}" titleStyle="{{isSharePoiValid ? '' : 'color:#F20707'}}" message="{{message}}" tt:if="{{poiList.length}}"><view class="mall-modal-items"><view class="mall-modal-item {{index===0?'first':''}} {{((selectedPoiIdStr && selectedPoiIdStr === item.poiIdStr) || (selectedPoiId && selectedPoiId===item.poiId)) ? 'selected':''}} {{item.poiBusinessStatus===2?'item-offline':''}}" bindtap="selectPoi" data-item="{{item}}" data-index="{{index}}" tt:for="{{poiList}}" tt:for-item="item" tt:key="index"><view class="{{item.poiBusinessStatus===2?'mall-modal-entirety':''}}" tt:if="{{item.poiBusinessStatus===2}}"></view><view class="poi-tag{{item.tagTxt === '距离最近'?'-distance':item.tagTxt === '暂停营业'?'-stop':'-common'}}" tt:if="{{item.tagTxt}}">{{item.tagTxt}}</view><view class="poi-name ellipsis">{{item.poiName}}</view><view class="poi-distance selected" tt:if="{{item.distanceText}}">{{item.distanceText}}</view><view class="poi-address" tt:if="{{item.address}}">{{item.address}}</view><image class="poi-self-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/selected_new.aaa1656.png" tt:if="{{(selectedPoiIdStr && selectedPoiIdStr === item.poiIdStr) || (selectedPoiId && selectedPoiId===item.poiId)}}"></image><view class="store-introduce" tt:if="{{coldStorage.length>0 || item.poiLabelsV2 && item.poiLabelsV2.length>0}}"><view class="cold-storage" tt:for="{{item.poiLabelsV2}}" tt:if="{{item.labelAttr =='2'}}">{{item.labelName}}</view><view class="service-tag-box" tt:if="{{coldStorage.length>0}}"><view class="service" tt:for="{{coldStorage}}" tt:if="{{item.labelAttr =='3'}}"><view class="vertical-line" tt:if="{{index!== 1}}"></view>{{item.labelName}}</view></view></view></view></view><view class="mall-modal-item-footer {{isSharePoiValid ? 'vertical' : 'horizontal'}}"><block tt:if="{{!isTwoButtonStyle}}"><view class="f-middle" bindtap="onModalButtonClick" data-url="{{leftUrl}}" data-action="middle">{{middleTxt}}</view></block><block tt:else><view class="{{isSharePoiValid ? 'f-bottom' : 'f-left'}}" bindtap="onModalButtonClick" data-url="{{leftUrl}}" data-action="left">{{leftTxt}}<image class="arrow-img" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/multi-poi-modal-arrow.5faf1018.png" tt:if="{{isSharePoiValid}}"></image></view><view class="{{isSharePoiValid ? 'f-top' : 'f-right'}} confirm-text" bindtap="onModalButtonClick" data-action="right">{{rightTxt}}</view></block></view></ob><on show="{{showInner}}" use-slot="{{true}}" can-close="{{false}}" can-close-on-click-backdrop="{{innerCanClose}}" z-index="{{1010}}" title="{{innerTitle}}" message="{{innerMessage}}" error-code="{{innerErrorCode}}" bindclose="closeNoLocationModal" tt:if="{{showInner}}"><view class="mall-modal-item-footer inner-poi-modal" tt:if="{{isHideLeftBtn}}"><button class="f-middle" bindtap="onInnerModalButtonClick" data-text="{{innerLeftTxt}}" data-status="{{innerStatus}}" data-action="cancel">{{innerLeftTxt}}</button></view><view class="mall-modal-item-footer inner-poi-modal" tt:else><view class="f-left" bindtap="onInnerModalButtonClick" data-text="{{innerLeftTxt}}" data-status="{{innerStatus}}" data-action="cancel">{{innerLeftTxt}}</view><button class="f-right" bindtap="onInnerModalButtonClick" data-text="{{innerRightTxt}}" data-action="openSetting" data-status="{{innerStatus}}" tt:if="{{innerOpenType==='openSetting'}}">{{innerRightTxt}}</button><view class="f-right" bindtap="onInnerModalButtonClick" data-text="{{innerRightTxt}}" data-status="{{innerStatus}}" data-action="confirm" tt:else>{{innerRightTxt}}</view></view></on><om show="{{showIPPOIModal}}" use-slot="{{true}}" can-close="{{false}}" can-close-on-click-backdrop="{{innerCanClose}}" z-index="{{1010}}" title="{{recommondPoiModalData.popTag}}" message="{{}}" bindclose="closeNoLocationModal" tt:if="{{showIPPOIModal}}"><view class="mall-modal-items accurate_location"><view class="ip-poi-content"><wq class="ip-poi-content-left-image" styleStr="border-radius: 8rpx;" webp-conf="{{({width: '160rpx', height: '160rpx'})}}" src="{{recommondPoiModalData.targetPoiInfo.picUrl}}" /><view class="ip-poi-content-right"><view class="ip-poi-content-name">{{recommondPoiModalData.targetPoiInfo.poiName}}</view><view class="ip-info-wrapper"><view class="desc" tt:if="{{recommondPoiModalData.targetPoiInfo.distanceText}}"><view class="text" tt:if="{{recommondPoiModalData.targetPoiInfo.distanceText}}">{{recommondPoiModalData.targetPoiInfo.distanceText}}</view></view><view class="vertical-line" tt:if="{{recommondPoiModalData.targetPoiInfo.distanceText && recommondPoiModalData.targetPoiInfo.address}}"></view><view class="addr" tt:if="{{recommondPoiModalData.targetPoiInfo.address}}">{{recommondPoiModalData.targetPoiInfo.address}}</view></view></view></view></view><view class="mall-modal-item-footer horizontal"><view class="f-middle horizontal" bindtap="onIPModalButtonClick" data-poi="{{recommondPoiModalData.targetPoiInfo}}">我知道了</view></view></om>