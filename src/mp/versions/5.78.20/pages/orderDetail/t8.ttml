<view class="rec-skus-wrap" tt:if="{{cartRecData.goodsList && cartRecData.goodsList.length}}"><view class="rec-sku-title" style="{{customTitleStyle}}"><text class="title-line" /><image src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/recomend_love.10d81068.png" class="res-sku-pic" />{{cartRecData.moduleTitle.text || '您可能还想买'}}<text class="title-line" /></view><view class="rec-sku-list"><block tt:for="{{cartRecData.goodsList}}" tt:for-index="index" tt:key="skuId"><view class="sku-item {{item.type !== 1 ? 'sku-item-cboard-oftenBuy' : ''}}"><block tt:if="{{item.type !== 1}}"><z customer-style="mv-wrapper" bid="{{skuSchemaMap[item.type]['bid']}}" lx-params="{{({ index_id: index, params: skuSchemaMap[item.type] })}}" bindmvReport="onBoardMvReport" tt:if="{{!isTT}}"><ub idx="{{index}}" type="{{item.type}}" sku-type="{{skuSchemaMap[item.type]['skuType']}}" title="{{item.moduleTitle}}" sub-title="{{item.subModuleTitle}}" sku-list="{{item.itemList}}" sku-seeMore="{{item.jumperVO}}" sku-schema="{{skuSchemaMap[item.type]['schema']}}" bind:seeMore="handleSeeMore" bind:addToCartCompletely="onAddToCartCompletely" bind:skuBoradsLxRegister="skuBoradsLxRegister"></ub></z></block><block tt:else><ux mvProps="{{mvProps}}" index="{{index}}" sku="{{item}}" schema="{{middle}}" bind:addToCartCompletely="onAddToCartCompletely" needAnimation="{{needAnimation}}" type="{{skuType}}" canDirect="{{skuCardCanDirect}}" bind:gotoSkuDetail="onSkuItemClick" /><view class="sku-item-divide-wrap" tt:if="{{index < cartRecData.goodsList.length - 1 && (cartRecData.goodsList[index + 1] && (cartRecData.goodsList[index + 1].type === 1 || cartRecData.goodsList[index + 1].type === 10))}}"><view class="sku-item-divide-line"></view></view></block></view></block><view class="guess-like-loading" tt:if="{{!isNoMoreCommand}}"><image class="guess-like-loading__icon" src="https://p0.meituan.net/mallimages/acfe9ca90c7b76e6ab5941752a05c2c52097.png" mode="scaleToFill"></image><text class="guess-like-loading__text">正在寻找你喜爱的商品</text></view><view class="already-bottom" tt:if="{{isNoMoreCommand}}">已经到底啦</view></view><uv height="110rpx"></uv></view>