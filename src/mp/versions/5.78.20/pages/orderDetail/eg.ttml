<view class="share-popup-cash-back"><tf show="{{popupVisible}}" show-mask="{{true}}" type="center" bind:maskclick="handleMaskClick" tt:if="{{popupVisible && rewardAmount}}"><view class="share-popup-cash-back__popup" bindtap="handleGoPopupShareLink"><wq class="share-popup-cash-back__popup-image" src="{{data.popPic}}" webp-conf="{{({height:'790rpx',width:'640rpx'})}}" /><view class="share-popup-cash-back__content"><view class="share-popup-cash-back__title">{{data.title}}</view><view class="share-popup-cash-back__price"><text class="share-popup-cash-back__price-character">￥</text>{{rewardAmount}}</view><view class="share-popup-cash-back__sub-title">{{data.subTitle}}</view></view></view></tf><view class="share-popup-cash-back__pendant {{isIpx?'fix-ipx':''}}" bind:tap="handleGoPendantShareLink" tt:if="{{!popupVisible && showPendant && data.bubblePic}}"><wq src="{{data.bubblePic}}" class="share-popup-cash-back__pendant-image" webp-conf="{{({height:'140rpx',width:'140rpx'})}}" /><view class="share-popup-cash-back__pendant-content"><text class="share-popup-cash-back__pendant-price">{{pendantRewardAmount}}</text><text class="share-popup-cash-back__pendant-character">元</text></view></view></view>