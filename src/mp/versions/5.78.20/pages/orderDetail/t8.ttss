.rec-skus-wrap{box-sizing:border-box;padding:0 0 68rpx;width:100%}.rec-sku-title{align-items:center;background-image:linear-gradient(1deg,#fff,#f5f6f7 93%);color:#121924;display:flex;font-size:32rpx;height:46rpx;justify-content:center;line-height:45rpx;padding:36rpx 0 6rpx;width:100%}.res-sku-pic{height:40rpx;margin-right:6rpx;width:40rpx}.sku-item{background-color:#fff}.sku-item-cboard-oftenBuy{height:auto;margin:12rpx 0;width:100%}.already-bottom{color:#95989e;font-size:26rpx;margin-top:46rpx;text-align:center}.title-line{border-bottom:2rpx solid hsla(215,6%,63%,.3);margin:0 24rpx;width:50rpx}.guess-like-loading{align-items:center;box-sizing:border-box;display:flex;height:132rpx;justify-content:center;padding:6rpx 0 30rpx}.guess-like-loading__icon{animation-duration:1s;animation-iteration-count:infinite;animation-name:guessLoadingAnimation;animation-timing-function:linear;height:30rpx;width:30rpx}.guess-like-loading__text{color:#b5b5b5;font-size:22rpx;margin-left:6rpx}.sku-item-divide-wrap{height:2rpx;overflow:hidden;text-align:right;width:100%}.sku-item-divide-line{background:rgba(0,0,0,.06);float:right;height:100%;width:408rpx}