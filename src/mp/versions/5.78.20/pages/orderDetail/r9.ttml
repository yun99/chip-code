<tf show="{{show}}" type="center" show-mask="{{show}}" catchtouchmove><view class="price-pop-container"><view class="close-btn close-btn-{{info.stayTypeCode}}" data-stay="{{true}}" bindtap="onClose"></view><view class="header" tt:if="{{info.stayTypeCode === 4 || info.stayTypeCode === 7}}">温馨提示</view><view class="container container-{{info.stayTypeCode}}"><view class="message" tt:if="{{(!info.stayTypeCode || info.stayTypeCode === 0 || info.stayTypeCode === 7) && info.message}}">{{info.message}}</view><view class="stock-container" tt:if="{{info.stayTypeCode === 1 && info.sellOut.skuPicInfos && info.sellOut.skuPicInfos.length}}"><view class="message">{{info.sellOut.message}}</view><view class="pic-list-container"><scroll-view class="pic-list" scroll-x><view class="pic-item pic-item-{{index}}" tt:for="{{info.sellOut.skuPicInfos || []}}" tt:key="skuId"><wq webp-conf="{{({width: '160rpx', height: '160rpx' })}}" src="{{item.skuPicUrl}}" /></view></scroll-view></view></view><view class="discount-message-container" tt:if="{{info.stayTypeCode === 2 && discountInfo && discountInfo.message}}"><view class="discount-text"><view class="left">{{discountInfo.preInfo}}</view><view class="middle">{{discountInfo.discountMoney}}</view><view class="right">{{discountInfo.unit}}</view></view><view class="info">{{discountInfo.nextInfo}}</view></view><view class="review-container" tt:if="{{info.stayTypeCode === 3 && info.skuReview.reviews && info.skuReview.reviews.length}}"><view class="message">{{info.skuReview.message}}</view><view class="review-pic-container"><view class="pic-wrapper"><wq webp-conf="{{({width: '200rpx', height: '200rpx' })}}" src="{{info.skuReview.skuPicUrl}}" lazyLoad /></view><view class="review-total-count">{{info.skuReview.totalReview}}人好评</view></view><view class="review-list"><view class="review-item" tt:for="{{info.skuReview.reviews || []}}" tt:key="reviewId"><view class="review-text">{{item.reviewMessage}}</view><view class="review-count">{{item.reviewCount}}</view></view></view></view><view class="contact-group-container" tt:if="{{info.stayTypeCode === 4 && info.contactGroup.message}}"><view class="message">{{info.contactGroup.message}}</view></view><view class="people-buy-container" tt:if="{{info.stayTypeCode === 6 && info.buyPeopleCount.message}}"><view class="message">{{info.buyPeopleCount.message}}</view><view class="people-buy-pic-container"><view class="pic-wrapper"><wq webp-conf="{{({width: '160rpx', height: '160rpx' })}}" src="{{info.buyPeopleCount.skuPicUrl}}" lazyLoad /></view></view></view></view><view class="footer"><to class="custom-button" custom-class="refund" type="primary" plian other-style="background: #FFFFFF;border: 1rpx solid #C4C7CC;" data-stay="{{false}}" bindbtnclick="onCancel">{{cancelText}}</to><to class="custom-button" type="primary" data-stay bindbtnclick="onOk">{{okText}}</to></view></view></tf>