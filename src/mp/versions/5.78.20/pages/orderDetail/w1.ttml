<view class="question-pop-container-a1" catchtouchmove><view class="header">{{title || '服务满意度调查'}}<image class="close-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/close-modal.6769125.png" bindtap="close"></image></view><view class="question-container"><view class="question-title"><text class="current-num">{{idx}}</text><text class="total-num">/{{count}}</text>{{question.content}}</view><view class="question-content"><view class="tip"><view class="tip-item tip-left"><image class="image bad" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/question-bad.2fc3546.png"></image>{{question.extraInfos[0]}}</view><view class="tip-item tip-right"><image class="image good" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/question-good.fb26549.png"></image>{{question.extraInfos[1]}}</view></view><view class="items"><view class="item {{selectData[question.index].code === item.code ? 'selected' : ''}}" data-item="{{item}}" bindtap="select" tt:for="{{normalOptions}}" tt:key="code">{{item.text}}</view></view></view><view class="button-container" tt:if="{{count === idx}}"><yh type="primary" disabled="{{disabled}}" custom-class="button" bindtap="submit">提交反馈</yh></view></view></view>