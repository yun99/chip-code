.pop-container{background:#f5f5f5;border-radius:24rpx 24rpx 0 0;width:100%}.pop-container .header{padding:0;text-align:center}.pop-container .header .title{color:#121924;display:inline-block;font-family:PingFang SC;font-size:36rpx;font-weight:500;line-height:110rpx;margin:0 auto;position:relative;text-align:center}.pop-container .header .close{height:25rpx;padding:30rpx;position:absolute;right:0;top:0;width:25rpx}.container{padding:0 24rpx 48rpx}.container .btn{align-items:center;background:#fff;border-radius:12rpx;display:flex;flex-direction:row;height:146rpx;padding:0 25rpx 0 30rpx}.container .btn+.btn{margin-top:12rpx}.container .btn .icon{height:42rpx;margin-right:12rpx;width:42rpx}.container .btn .title{color:#121924;flex:1;font-family:PingFangSC-Medium;font-size:36rpx;font-weight:500;letter-spacing:0;line-height:50rpx}.container .btn .title .subTitle{color:rgba(0,0,0,.5);font-family:PingFang SC;font-size:28rpx;font-weight:400;letter-spacing:0;line-height:40rpx;margin-top:6rpx;width:100%}.container .btn .right{align-items:center;display:inline-flex;justify-content:center}.container .btn .right .arrow{height:20rpx;width:20rpx}.container .btn .contact-btn{align-items:center;border:1rpx solid rgba(0,0,0,.5);border-radius:100rpx;display:inline-flex;height:80rpx;justify-content:center;margin-right:5rpx;width:80rpx}.container .btn .contact-btn+.contact-btn{margin-left:37rpx}.container .btn .contact-btn .icon{height:32rpx;margin:0;width:32rpx}.fix-iphonex-bottom-fixed{background:#f5f5f5}