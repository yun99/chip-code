<view class="question-pop-container-a2" catchtouchmove><image class="close-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/question_pop_close.3000547.png" bindtap="close"></image><view class="question-title"><text class="current-num">{{idx}}</text><text class="total-num">/{{count}}</text>{{question.content}}</view><view class="items"><view class="item" data-item="{{item}}" bindtap="select" tt:for="{{normalOptions}}"><image class="item-icon" src="{{selectData[question.index].code === item.code ? item.picUrls[1] : item.picUrls[0]}}"></image><image class="item-icon hidden" src="{{selectData[question.index].code !== item.code ? item.picUrls[1] : item.picUrls[0]}}"></image><view class="text">{{item.text}}</view></view></view><view class="tips"><view class="tip {{selectData[question.index].code === item.code ? 'select' : ''}}" data-item="{{item}}" bindtap="select" tt:for="{{bottomOptions}}" tt:key="code">{{item.text}}</view></view><view class="button-container" tt:if="{{count === idx}}"><yh type="primary" disabled="{{disabled}}" custom-class="button" bindtap="submit">提交反馈</yh></view></view>