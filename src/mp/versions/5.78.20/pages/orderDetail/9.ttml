<view class="skus"><view class="delivery-tips" tt:if="{{deliveryTips}}"><yy bid="b_youxuan_jrqjhvn7_mv" lxParams="{{({order_id: orderId, stack_id: '', message: deliveryTips.content, type: deliveryTips.type, num: deliveryTips.abnormalSkuQuantity, sku_list: deliveryTips.abnormalSkuIds})}}" bindmvReport="mvHandler"><ts tips="{{deliveryTips}}" /></yy></view><block tt:for="{{renderSkus || []}}" tt:for-item="sku" tt:key="skuId" tt:if="{{index < 3 || showMore}}"><view data-not-can-jump="{{sku.notCanJump}}" data-type="{{sku.orderSkuItemType}}" data-poi-id="{{sku.poiId}}" data-sku-id="{{sku.skuId}}" data-sku="{{sku}}" bindtap="gotoSkuDetail"><iq from="orderDetail" is-last="{{index === renderSkus.length - 1}}" no-show-more sku-item="{{sku}}" extraBtn="{{sku.extraBtn}}" orderId="{{orderId}}" orderType="{{sku.orderSkuItemType}}" ext-cls="order-detail-sku" /></view><yy bid="b_youxuan_ymprylrz_mv" lxParams="{{({ a: 'custom' })}}" bindmvReport="mvHandler" tt:if="{{sku && sku.orderRefundTipVO && (sku.orderRefundTipVO.day || sku.orderRefundTipVO.hour || sku.orderRefundTipVO.minutes)}}"><view class="price-refund-modal" data-sku="{{sku}}"><image class="floatGreyArrow" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/orderDetailGreyArrow.c0c4511.png" /><view class="refundTItle">因为您对该商品申请了退款</view>请在<text class="timeStr">{{sku.orderRefundTipVO.day}}天{{sku.orderRefundTipVO.hour}}小时{{sku.orderRefundTipVO.minutes}}分</text>内将商品退还给团长</view></yy><yy bid="b_youxuan_m6ji025f_mv" lxParams="{{({ order_id: orderId, sku_id: sku.skuId })}}" bindmvReport="mvHandler" tt:if="{{sku.priceInsurance && !sku.orderRefundTipVO}}"><view class="price-insurance" data-sku="{{sku}}" bindtap="onShowPriceInsurancePop"><image class="floatYellowArrow" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/orderDetailYellowArrow.91d8517.png" /><view class="title">优选价格保护<image class="question" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/question-yellow.a1a71066.png" /></view>{{sku._priceInsuranceStr.prefix}}<text class="highlight">{{sku._priceInsuranceStr.highlight}}</text>{{sku._priceInsuranceStr.suffix}}</view></yy></block><view class="more" bindtap="toggleShowMore" tt:if="{{renderSkus.length > 3}}"><view class="text">{{showMore ? '收起' : '展开(共'+renderSkus.length+'件)'}}</view><image class="arrow {{showMore ? 'top' : 'bottom'}}" style="width: 20rpx; height: 20rpx;" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/orderDetailSanjiao3.2080516.png" /></view><oc show="{{showPriceInsurancePop}}" bindclose="onShowPriceInsurancePop" /></view>