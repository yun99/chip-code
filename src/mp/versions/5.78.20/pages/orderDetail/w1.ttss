.question-pop-container-a1 .header{border-bottom:1px solid #eee;color:#121924;font-size:30rpx;letter-spacing:0;padding:30rpx 0;position:relative;text-align:center}.question-pop-container-a1 .header .close-icon{height:23rpx;padding:30rpx;position:absolute;right:0rpx;top:10rpx;width:23rpx}.question-pop-container-a1 .question-title{color:#121924;font-family:PingFangSC-Medium;font-size:34rpx;font-weight:700;padding:40rpx 30rpx 80rpx}.question-pop-container-a1 .question-title .current-num{color:#121924;font-size:40rpx;font-weight:400}.question-pop-container-a1 .question-title .total-num{color:#bcbdbf;font-size:24rpx;font-weight:400;margin-right:10rpx}.question-pop-container-a1 .tip{color:#585d66;display:flex;font-size:26rpx;justify-content:space-between;padding:0 32rpx 18rpx}.question-pop-container-a1 .tip .tip-item{align-items:center;display:flex}.question-pop-container-a1 .tip .tip-item .image{height:30rpx;margin-right:8rpx;width:30rpx}.question-pop-container-a1 .items{display:flex;justify-content:space-between;padding:0 32rpx 180rpx}.question-pop-container-a1 .items .item{align-items:center;background:#fff;border:2rpx solid rgba(255,94,36,.5);border-radius:12rpx;box-sizing:border-box;color:#ff5e24;display:flex;font-size:32rpx;height:86rpx;justify-content:center;margin-right:6rpx;width:57rpx}.question-pop-container-a1 .items .item:last-child{margin-right:0}.question-pop-container-a1 .button-container{margin:0 auto;padding-bottom:30rpx;width:690rpx}