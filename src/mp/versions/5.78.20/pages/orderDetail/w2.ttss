.question-pop-container-b .edit-container{border:1.5rpx solid rgba(18,25,36,.1);border-radius:16rpx;margin:20rpx 30rpx;overflow:hidden}.question-pop-container-b .header{border-bottom:1px solid #eee;color:#121924;font-size:30rpx;letter-spacing:0;padding:30rpx 0;position:relative;text-align:center}.question-pop-container-b .header .close-icon{height:23rpx;padding:30rpx;position:absolute;right:0rpx;top:10rpx;width:23rpx}.question-pop-container-b .question-title{color:#121924;font-family:PingFangSC-Medium;font-size:34rpx;font-weight:700;padding:40rpx 30rpx 30rpx}.question-pop-container-b .question-title .current-num{color:#121924;font-size:40rpx;font-weight:400}.question-pop-container-b .question-title .total-num{color:#bcbdbf;font-size:24rpx;font-weight:400;margin-right:10rpx}.question-pop-container-b .tip{color:#585d66;display:flex;font-size:26rpx;justify-content:space-between;padding:0 32rpx 18rpx}.question-pop-container-b .items{display:flex;flex-wrap:wrap;padding-left:28rpx}.question-pop-container-b .items .item{align-items:center;background:#f5f6f7;border-radius:12rpx;box-sizing:border-box;color:#3f444d;display:flex;flex:none;font-size:24rpx;height:60rpx;justify-content:center;margin-bottom:16rpx;margin-right:16rpx;width:220rpx}.question-pop-container-b .items .item:last-child{margin-right:0}.question-pop-container-b .items .item.selected{background:#fff2ef;color:#ff5e24;font-family:PingFangSC-Medium;font-size:24rpx;font-weight:700}.question-pop-container-b .button-container{background:#fff;margin:0 auto;padding:20rpx 0 30rpx;width:690rpx}