.status-card-box{height:90rpx;left:0;position:relative;top:0;width:100vw}.status-card{text-align:center}.status-card-position{background:#fff;height:90rpx;left:0rpx;position:relative;top:0;width:100vw;z-index:99}.status-card-position.fixed{position:fixed}.status-icon{flex-shrink:0;height:40rpx;margin-right:12rpx;width:40rpx}.title{align-items:center;display:inline-flex;padding:18rpx 0 24rpx}.title .text{color:#191919;display:inline-block;font-family:PingFangSC-Semibold;font-size:34rpx;font-weight:700;letter-spacing:0;line-height:50rpx}.title.padding22{padding:22rpx 0}.sub-title{color:#191919;display:block;font-family:PingFangSC-Regular;font-size:28rpx;font-weight:400;letter-spacing:0;line-height:40rpx;margin-top:8rpx}.process{display:flex;flex-direction:row;padding:8rpx 0 24rpx}.process .process-item{box-sizing:border-box;color:rgba(0,0,0,.9);flex:1;font-size:28rpx;line-height:40rpx;text-align:center}.process .process-item:first-child .process-line.left,.process .process-item:last-child .process-line.right{visibility:hidden}.process .process-item.state-0{color:rgba(0,0,0,.35)}.process .process-item.state-0 .process-line{background:rgba(0,0,0,.06)}.process .process-item.state-0 .process-icon .inner-icon{background:#f0f0f0}.process .process-item.state-1 .process-line.left{background:#ffd100}.process .process-item.state-1 .process-line.right{background:rgba(0,0,0,.06)}.process .process-item.state-1 .process-icon{background-color:#fff5c7}.process .process-item.state-1 .process-icon .inner-icon,.process .process-item.state-2 .process-line{background:#ffd100}.process .process-item.state-2 .process-icon .inner-icon{border:3rpx solid #ffd100}.process .process-item .process-node{align-items:center;display:flex;justify-content:center;margin-bottom:6rpx}.process .process-item .process-node .process-line{flex:1;height:4rpx}.process .process-item .process-node .process-icon{border-radius:50%;height:24rpx;position:relative;width:24rpx}.process .process-item .process-node .process-icon .inner-icon{border-radius:50%;box-sizing:border-box;height:16rpx;left:50%;position:absolute;top:50%;transform:translate(-50%,-50%);width:16rpx}