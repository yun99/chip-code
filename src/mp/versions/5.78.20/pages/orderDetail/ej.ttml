<view class="share-popup"><ed show="{{canShowPop}}" type="center" bind:clickmask="closeDropMask"><view class="pop-content"><wq class="bitc-image" src="{{popObj.picUrl || defaultPic}}" webp-conf="{{({height:'790rpx',width:'640rpx'})}}" bindtap="shareTapHandle" /><view class="close-view"><image src="https://p0.meituan.net/mallimages/9c1ca6753db5595d3422e323dfd805be2198.png" class="drop-close-button" bindtap="closeDropMask"></image></view><button open-type="share" class="share-btn" data-message="{{shareMessage}}" bindtap="shareTapHandle" tt:if="{{popToShare}}"></button></view></ed><view class="share {{popObj.type === 1 ? 'share-invite' : ''}} {{isIpx?'fix-ipx':''}} {{popUpBottom === 190 ? 'fix-bottom' : ''}}" bindtap="bubbleTapHandle" tt:if="{{!canShowPop && showBubble && bubblePic}}"><wq class="share-image" src="{{bubblePic || defaultPic}}" webp-conf="{{({height:popObj.type === 1?'115rpx':'140rpx',width:popObj.type === 1?'90rpx':'140rpx'})}}" /><button class="share-button" open-type="share" data-message="{{shareMessage}}" tt:if="{{bubbleToShare}}" /></view></view>