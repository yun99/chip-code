.deliveryTips{background:#fafafa;border-radius:8rpx;display:flex;padding:6rpx 12rpx}.deliveryTips .content{-webkit-box-orient:vertical;-webkit-line-clamp:1;color:#95989e;display:-webkit-box;flex:1;font-size:24rpx;line-height:40rpx;overflow:hidden;text-overflow:ellipsis;white-space:break-spaces;word-break:break-all}.deliveryTips .content .icon{background:#ed0010;border-radius:6rpx;color:#fff;display:inline-block;font-size:22rpx;height:30rpx;line-height:30rpx;margin-right:8rpx;text-align:center;width:30rpx}.deliveryTips .content .title{color:#121924;display:inline-block;font-size:28rpx;font-weight:400;line-height:40rpx;margin-right:8rpx;vertical-align:middle}.deliveryTips .expand-icon{height:20rpx;margin:16rpx 0 0 10rpx;width:20rpx}.deliveryTips.expand .content{-webkit-line-clamp:2}.deliveryTips.expand .expand-icon{transform:rotate(180deg)}