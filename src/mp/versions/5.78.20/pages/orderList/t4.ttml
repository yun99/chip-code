<view class="often-buy-container" data-jump-url="{{oftenBuyData.clickText && oftenBuyData.clickText.iretail || ''}}" bindtap="oftenBuyClick" tt:if="{{oftenBuyShow}}"><view class="often-buy-left"><image class="often-buy-title" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/often_buy.6963498.png" /><view class="down"><text class="prefix-content">{{oftenBuyData.textPrefix || ''}}</text><text class="text-content">{{oftenBuyData.oftenBuyContent}}</text><text class="suffix-content">{{oftenBuyData.textSuffix || ''}}</text></view></view><view class="often-buy-right"><view class="often-buy-images" tt:if="{{oftenBuyData.skuPicVOs.length > 0}}"><view class="sku-img" tt:for="{{oftenBuyData.skuPicVOs}}" tt:key="picUrl"><wq class="sku-img-pic" style-str="border-radius: 10rpx;" src="{{item.picUrl || 'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/sku_default_108_108.41b6712.png'}}" webp-conf="{{({width:'80rpx',height:'80rpx'})}}" /><view class="sale-tag" tt:if="{{item.saleTag && item.saleTag.text && item.saleTag.text.length < 7}}">{{item.saleTag.text}}</view></view><image class="decorate-img" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/often-buy-red-arrow.6c3d500.png"></image></view><view class="often-buy-btn" tt:else>去看看<image class="often-buy-arrow" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/often-buy-arrow.a596495.png" /></view></view></view>