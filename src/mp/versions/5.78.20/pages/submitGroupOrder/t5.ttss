@import "../../jn.ttss";.popup{align-items:flex-end;display:flex;flex-direction:column}.popup_content{background:#fff;border-radius:24rpx;color:rgba(0,0,0,.9);flex-direction:column;font-size:36rpx;margin-top:60rpx;padding:30rpx;width:590rpx}.popup_content_title{display:flex;font-weight:700;line-height:60rpx;margin-bottom:30rpx}.popup_content_title_price{color:#f20000}.popup_content_pic-box{margin-bottom:12rpx;position:relative}.popup_content_pic-box_label{background-image:linear-gradient(90deg,#ff2d19,#f50);border-radius:12rpx 0 12rpx 0;color:#fff;font-size:26rpx;left:0;padding:4rpx 12rpx;position:absolute;top:0}.popup_content_name{color:#191919;font-size:30rpx;line-height:42rpx;margin-bottom:6rpx}.popup_content_price{align-items:baseline;color:#f20000;display:flex;font-weight:700;margin-bottom:30rpx}.popup_content_price_symbol{font-size:26rpx;margin-right:4rpx}.popup_content_price_num{font-size:44rpx;line-height:62rpx;margin-right:6rpx}.popup_content_price_unit{color:rgba(0,0,0,.35);font-size:30rpx;font-weight:400;transform:translateY(-2rpx)}.popup_content_butt{background:#ffd100;border-radius:46rpx;color:rgba(0,0,0,.8);font-size:34rpx;font-weight:700;height:90rpx;justify-content:center;width:100%}.popup_content_butt:after{border:unset}