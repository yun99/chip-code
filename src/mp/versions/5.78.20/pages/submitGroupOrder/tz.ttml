<tf show="{{show}}" type="center" show-mask="{{show}}" z-index="210" catchtouchmove><view class="redemption-popup" catchtouchmove><view class="redemption-popup__header"><view class="sub-title">真的要放弃下单吗？</view><view class="main-title">{{redemptionData.title}}</view></view><view class="redemption-popup__body"><view class="redemption-popup__sku-image"><wq src="{{skuItem.pic}}" webp-conf="{{({height:'380rpx', width:'380rpx'})}}" styleStr="display:block;border-radius:12rpx;border: 1rpx solid #eee;box-sizing: border-box;" /><view class="redemption-popup__limit-tag" tt:if="{{redemptionData.itemTag}}">{{redemptionData.itemTag}}</view></view><view class="redemption-popup__sku-name"><wq class="redemption-popup__activity-tag" src="{{skuItem.activityTag}}" mode="heightFix" webp-conf="{{({height:'30rpx'})}}" tt:if="{{skuItem.activityTag}}" />{{skuItem.skuName}}</view><view class="redemption-popup__sku-price"><view class="redemption-popup__price"><view class="text">¥</view><view class="text redemption-popup__new-price">{{skuItem.newPrice}}</view><view class="text redemption-popup__unit" tt:if="{{skuItem.unit}}">/{{skuItem.unit}}</view><view class="text redemption-popup__old-price" tt:if="{{skuItem.oldPrice}}">¥{{skuItem.oldPrice}}</view></view></view></view><view class="redemption-popup__footer"><view class="redemption-popup__footer-btn {{'redemption-popup__footer-btn--' + item.code}}" data-code="{{item.code}}" bind:tap="handleBtnClick" tt:for="{{redemptionData.buttonList}}" tt:key="code">{{item.desc}}</view></view></view></tf>