.redemption-popup{background-color:#fff;border-radius:24rpx;box-sizing:border-box;overflow:hidden;padding:36rpx 40rpx;width:590rpx}.redemption-popup__header{color:#121924;font-size:34rpx;font-weight:500;margin-bottom:34rpx;text-align:center}.redemption-popup__body{align-items:center;display:flex;flex-direction:column;margin-bottom:22rpx}.redemption-popup__sku-image{height:350rpx;margin-bottom:24rpx;overflow:hidden;position:relative;width:350rpx}.redemption-popup__limit-tag{align-items:center;background-image:linear-gradient(-135deg,#ff5e24,#ff1929);border-radius:18rpx 18rpx 18rpx 2rpx;color:#fff;display:flex;font-size:24rpx;height:36rpx;justify-content:center;left:0;padding:0 10rpx;position:absolute;top:0}.redemption-popup__sku-name{color:#121924;font-size:30rpx;text-align:center}.redemption-popup__sku-price{align-items:center;display:flex;justify-content:center}.redemption-popup__price{color:#ff1929;font-size:44rpx}.redemption-popup__price .text{display:inline-block}.redemption-popup__price .text:first-child{font-size:30rpx;margin-right:3rpx}.redemption-popup__new-price{font-weight:500}.redemption-popup__unit{color:#95989e;font-size:24rpx}.redemption-popup__old-price{color:#95989e;font-size:30rpx;margin-left:17rpx;text-decoration:line-through}.redemption-popup__footer{align-items:center;display:flex;justify-content:space-between}.redemption-popup__footer-btn{background-color:#f2f4f5;border-radius:40rpx;color:#585d66;flex:1;font-size:30rpx;line-height:80rpx;overflow:hidden;text-align:center;text-overflow:ellipsis;white-space:nowrap}.redemption-popup__footer-btn:not(:last-child){margin-right:24rpx}.redemption-popup__footer-btn--1{background-image:linear-gradient(-135deg,#ff5e24,#ff1929);color:#fff}