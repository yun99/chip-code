<tf show="{{show}}" type="center" show-mask="{{show}}" z-index="210" bind:maskclick="closePopup" catchtouchmove><view class="redemption-popup" catchtouchmove><view class="redemption-popup__header"><text>{{redemptionData.title}}</text></view><view class="redemption-popup__body"><view class="redemption-popup__sku-image"><wq src="{{skuItem.pic}}" webp-conf="{{({height:'380rpx', width:'380rpx'})}}" styleStr="display:block;border-radius:12rpx;border: 1rpx solid #eee;box-sizing: border-box;" /><view class="redemption-popup__limit-tag" tt:if="{{skuItem.promotionLimitDesc}}">{{skuItem.promotionLimitDesc}}</view></view><text class="redemption-popup__sku-name">{{skuItem.skuName}}</text><view class="redemption-popup__sku-price"><yf tag-entity="{{tagEntity}}" tt:if="{{tagEntity.text}}" /><view class="redemption-popup__price"><view class="text">¥</view><view class="text redemption-popup__new-price">{{skuItem.newPrice}}</view><view class="text redemption-popup__unit" tt:if="{{skuItem.feUnit}}">/{{skuItem.feUnit}}</view><view class="text redemption-popup__old-price" tt:if="{{skuItem.oldPrice}}">¥{{skuItem.oldPrice}}</view></view></view></view><view class="redemption-popup__footer"><view class="redemption-popup__footer-btn {{'redemption-popup__footer-btn--' + item.code}}" data-code="{{item.code}}" bind:tap="handleBtnClick" tt:for="{{redemptionData.buttonList}}" tt:key="code">{{item.desc}}</view></view></view></tf>