@import "../../ac.ttss";.pay-type-popup{background:#fff;border-radius:24rpx 24rpx 0 0;height:fit-content;width:750rpx}.pay-type-popup .container{padding:0 30rpx}.pay-type-popup .container .pay-item{border-bottom:1rpx solid #eee;padding:30rpx 0}.pay-type-popup .container .pay-item:last-child{border:none}.pay-type-popup .container .pay-item.active .title{color:#ff6000;font-family:PingFangSC-Medium;font-weight:500}.pay-type-popup .container .pay-item .title{display:inline-block;font-size:34rpx;line-height:46rpx}.pay-type-popup .container .pay-item .title .sub{font-size:26rpx;line-height:36rpx}.pay-type-popup .container .pay-item .title .sub .highlight{color:#ed1419}.pay-type-popup .container .pay-item .image{height:38rpx;width:38rpx}.pay-type-popup .container .pay-item.disabled .title,.pay-type-popup .container .pay-item .title .sub{color:#95989e;font-weight:400}.pay-type-popup .cancel-button{align-items:center;background:#fff;box-sizing:border-box;display:flex;flex-direction:column;justify-content:center;margin-top:30rpx;padding:10rpx 30rpx 30rpx;width:100%}.pay-type-popup .cancel-button-text{background:#fff;border:1px solid #c4c7cc;border-radius:86rpx;box-sizing:border-box;flex:none;font-size:32rpx;height:86rpx;line-height:86rpx;text-align:center;width:100%}