.notice-board{align-items:center;background-color:#fffbe4;border-radius:18rpx;box-sizing:border-box;display:flex;font-size:24rpx;margin:24rpx 18rpx 0;max-height:124rpx;min-height:64rpx;padding:16rpx 14rpx 16rpx 24rpx;position:relative;z-index:99}.notice-board-close{align-items:center;display:flex;height:32rpx;justify-content:center;width:42rpx}.notice-board-close-image{height:22rpx;width:22rpx}.notice-board-text{-webkit-box-orient:vertical;-webkit-line-clamp:2;color:#be3503;display:-webkit-box;flex:1;font-size:24rpx;margin-right:-2rpx;overflow:hidden;text-overflow:ellipsis}.notice-board-text.ORDER_DETAIL{color:rgba(51,51,51,.98);margin-left:16rpx;margin-right:0rpx}.notice-board-text.three{-webkit-line-clamp:3}.bell-pic{font-size:0;height:40rpx;width:40rpx}.notice-container{background-color:#fff;border-radius:20rpx;display:flex;flex-direction:column;padding-top:40rpx;width:630rpx}.notice-container .notice{align-items:center;box-sizing:border-box;color:#333;display:flex;font-size:28rpx;justify-content:center;margin-bottom:40rpx;max-height:360rpx;padding:0 40rpx;text-align:center;width:630rpx}.notice-container .notice-close{align-items:center;border-top:1rpx solid #e1e1e1;color:#ff5e24;display:flex;font-size:26rpx;height:98rpx;justify-content:center}