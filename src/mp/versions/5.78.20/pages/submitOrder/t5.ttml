<tf show="{{isShow}}" type="center" z-index="{{zIndex}}" catchtouchmove><z bind:mvReport="onMvReport" noData><view class="popup"><wq src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-extra-popup-close.4f3f909.png" webp-conf="{{({width: '48rpx', height: '48rpx' })}}" styleStr="display: block;" bind:tap="onPopupClose" /><view class="flex-align-center popup_content"><view class="popup_content_title">{{titleArr[0]}}<view class="popup_content_title_price">{{titleArr[1]}}</view>{{titleArr[2]}}</view><view class="popup_content_pic-box"><view class="popup_content_pic-box_label">凑单专享</view><wq src="{{popProduct.pic}}" webp-conf="{{({width: '380rpx', height: '380rpx' })}}" styleStr="display: block;border-radius: 12rpx;border: 2rpx solid #F0F0F0" /></view><view class="popup_content_name">{{popProduct.productName}}</view><view class="popup_content_price"><view class="popup_content_price_symbol">¥</view><view class="popup_content_price_num">{{popProduct.sellPrice / 100}}</view><view class="popup_content_price_unit">/{{popProduct.unit}}</view></view><button class="flex-align-center popup_content_butt" bind:tap="onBuyButtClick">{{buttonName}}</button></view></view></z></tf>