.pay-view{align-items:center;background:#fff;border-top:1rpx solid #f0f0f0;box-sizing:border-box;display:flex;justify-content:flex-end;left:0;padding:0 24rpx;position:fixed;width:100%;z-index:95}.pay-view,.pay-view-2{height:108rpx}.pay-view-3{height:118rpx}.pay-view .fee{align-items:center;display:flex;flex:1;flex-direction:row-reverse;justify-content:space-between;margin-right:24rpx}.pay-view .fee .total-money{align-items:baseline;display:flex}.pay-view .fee .total-money .text{color:#ed1419;display:inline-block;font-size:40rpx;line-height:56rpx}.pay-view .fee .total-money .text.price-unit{font-size:28rpx;line-height:1;margin-right:4rpx}.pay-view .fee .total-money .text.price-text{color:#121924;font-family:PingFangSC-Regular;font-size:28rpx;font-weight:400;line-height:1;margin-right:6rpx}.pay-view .fee .total-money .text-2{line-height:68rpx}.pay-view .fee .total-money .text-3{line-height:78rpx}.pay-view .fee-2{margin-right:24rpx}.pay-view .fee-3{margin-right:21rpx}.template-button{background:transparent;border-radius:2rpx!important;color:transparent;line-height:1;margin:0;padding:0}.template-button:after{border:none}.template-button .pay{background:#ffd100;border-radius:40rpx;color:rgba(0,0,0,.8);font-family:PingFangSC-Medium;font-size:32rpx;height:80rpx;line-height:80rpx;padding:0;position:relative;width:260rpx;z-index:10}.template-button .pay:after{border-color:#ffd100}.template-button .pay.diable-pay{background:#f5f6f7!important;border-radius:54rpx;color:#b0b3b8!important;font-family:PingFangSC;font-weight:400;z-index:auto}.pay-view-placeholder{background-image:linear-gradient(-180deg,hsla(0,0%,100%,0) 22%,hsla(0,0%,100%,.6) 35%,#fff 40%,#fff);bottom:0;position:fixed;width:100%}