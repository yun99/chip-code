.text{display:inline}.detention-popup{background:#fff;border-radius:24rpx;box-sizing:border-box;overflow:hidden;padding:0;width:650rpx}.detention-popup__header{color:rgba(0,0,0,.9);font-family:PingFangSC-Medium;font-size:34rpx;font-weight:700;line-height:48rpx;margin:36rpx auto 0;text-align:center;width:395rpx}.detention-popup__body{display:flex;justify-content:center;margin:24rpx 0 12rpx;padding:0 30rpx}.detention-popup__sku-pic{border:1px solid #f0f0f0;border-radius:8rpx;overflow:hidden}.detention-popup__sku-pic .image{display:block;font-size:0}.detention-popup__sku--2:not(:last-child){margin-right:24rpx}.detention-popup__sku--3:not(:last-child){margin-right:19rpx}.detention-popup__sku-price{align-items:baseline;color:#f20000;display:flex;font-weight:500;justify-content:center;margin-top:12rpx}.detention-popup__symbol{font-size:26rpx;line-height:42rpx;margin-right:4rpx}.detention-popup__new-price{font-size:34rpx;line-height:48rpx}.detention-popup__sku--1 .detention-popup__new-price{font-size:42rpx;line-height:62rpx}.detention-popup__footer{align-items:center;background:#fff;box-sizing:border-box;display:flex;justify-content:space-between;padding:30rpx}.detention-popup__footer-btn{border:2rpx solid rgba(0,0,0,.12);border-radius:45rpx;box-sizing:border-box;color:rgba(0,0,0,.8);font-size:34rpx;font-weight:400;line-height:90rpx;overflow:hidden;text-align:center;text-overflow:ellipsis;white-space:nowrap;width:286rpx}.detention-popup__footer-btn--7{background:#ffd100;border:none;font-family:PingFangSC-Medium;font-weight:700}