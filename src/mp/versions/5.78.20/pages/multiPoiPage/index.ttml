<wxs module="fps" src="../../ij.sjs" /><include src="../../er.ttml" /><view class="my-page-wrapper {{cityUnOpen ? 'yellow-theme':''}}" tt:if="{{dataReady}}"><ri theme="{{cityUnOpen ? 'yellow-theme':''}}"><block tt:if="{{cityUnOpen}}"><view class="background-area" slot="top"><wq class="left-icon" webp-conf="{{({width:'750rpx', height:'600rpx'})}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/multiPoiPage_cityUnOpen_bg.7a031208.jpg" mode="widthFix"></wq></view><view class="my-page-content my-page-before yellow-theme" slot="before"><view class="city-unopen"><view class="city-unopen-title">{{cityUnOpenInfo.title}}</view><view class="city-unopen-sub-title" bindtap="toSelectLocation"><text>{{cityUnOpenInfo.subTitle}}</text><image class="city-unopen-right-icon" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/c-mp/files/2_migrate-4594641eba35d177ce25fac2febc903d.png" mode="widthFix" tt:if="{{!!cityUnOpenInfo.protocol}}"></image></view></view></view><view class="my-page-content my-page-middle yellow-theme" slot="middle"><ro current-adress="{{currentAddress}}" city-un-open-info="{{cityUnOpenInfo}}" type="{{type}}"></ro></view></block><block tt:if="{{!cityUnOpen && areaUnOpen}}"><view class="my-page-content my-page-before" slot="before"><ro id="multi-poi-open" current-adress="{{currentAddress}}" poi-open-infos="{{poiOpenInfos}}" type="{{type}}"></ro></view><view class="my-page-content my-page-middle" slot="middle" tt:if="{{poiOpenInfos.poiInfos.length}}"><view class="my-page-title bottom-border">为您精选{{poiOpenInfos.poiInfos.length}}家自提站</view><block tt:for="{{poiOpenInfos.poiInfos}}" tt:key="index" tt:for-item="poiInfo"><ra poi-info="{{poiInfo}}" index="{{index}}" right-text="进店逛逛" type="{{type}}"></ra></block></view></block><view class="my-page-content my-page-after margin-bottom {{cityUnOpen ? 'yellow-theme':''}}" customer-style="border:1rpx solid red;" slot="after"><view class="my-page-title border-bottom" tt:if="{{!cityUnOpen}}">加入美团优选</view><block tt:for="{{hrInfos}}" tt:key="index" tt:for-item="item"><rs type="{{item.type}}" left-logo="{{item.picUrl}}" status="{{type}}" protocol='{{item.jumpUrl}}' with-border="{{false}}" customer-style="{{type===0 && index!==0? 'yellow-border': index!==0 ? 'border-top':''}}"><view class="hr-item"><view class="hr-title">{{item.title}}</view><view class="hr-desc">{{item.subTitle}}</view></view></rs></block></view></ri></view><view change:prop="{{fps.fpsRecord}}" prop="{{_fpsIsComputed}}"></view>