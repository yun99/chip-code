<wxs module="tools" src="../../ji.sjs" /><view class="brandEntryContainer base-style" bind:tap="gotoBrandDetail"><z noData bind:mvReport="onMvReport"><view class="flex items-center justify-between"><view class="flex"><wq src="{{brandAreaVO.logoUrl}}" webpConf="{{({width: '80rpx', height: '80rpx'})}}" styleStr="border-radius: 10%;" isBlockImg tt:if="{{brandAreaVO.logoUrl}}" /><view class="titleArea flex flex-col justify-between g-text-3 g-color-4"><view class="title">{{tools.getDataByPath(brandAreaVO, 'brandName.text')}}</view><view>{{tools.getDataByPath(brandAreaVO, 'skuNumOnSaleText.text') || '更多商品推荐'}}</view></view></view><view class="flex items-center"><view class="g-text-3 g-color-4">{{tools.getDataByPath(brandAreaVO, 'moreUrl.text')}}</view><wq src="https://p1.meituan.net/groceryimages/e8737c269592470d3321e2320892f6cf297.png" webpConf="{{({width: '22rpx', height: '22rpx'})}}" isBlockImg /></view></view><view class="bottom" tt:if="{{brandAreaVO.brandBanner && brandAreaVO.brandBanner.url}}"><wq src="{{tools.getDataByPath(brandAreaVO, 'brandBanner.url')}}" styleStr="border-radius: 20rpx;" webpConf="{{bannerWebpConf}}" isBlockImg /></view></z></view>