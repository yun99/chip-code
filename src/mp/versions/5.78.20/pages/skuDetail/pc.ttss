.thumb-item{align-items:center;box-sizing:border-box;display:flex;justify-content:center}.thumb-icon{height:34rpx;width:34rpx}.with-txt .thumb-item{height:50rpx}.with-txt .thumb-icon-wrap{align-items:center;display:flex;height:44rpx;justify-content:center;width:50rpx}.with-txt .thumb-text{flex-shrink:0;font-size:24rpx;line-height:44rpx}.with-txt.good.active .thumb-text{color:#ff5e24}.is-btn.no-txt .thumb-item{border-radius:30rpx;height:54rpx;width:54rpx}.is-btn.with-txt .thumb-item{background:#f2f2f2;border-radius:37rpx;height:74rpx;width:230rpx}.is-btn.with-txt .thumb-text{font-size:30rpx;margin-left:8rpx}.is-btn.with-txt.bad,.is-btn.with-txt.good{color:#666}.is-btn.with-txt.active{font-weight:700}.is-btn.good.active .thumb-item{background:#fff2ef;border:1px solid #ff5e24}.is-btn.good.animation .thumb-icon{animation:thumb-good .8s}.is-btn.bad.active .thumb-item{background:#dce1e2;border:1px solid #b4b4b4}.is-btn.bad.animation .thumb-icon{animation:thumb-bad .8s}@keyframes thumb-good{0%{height:34rpx;transform:rotate(0);width:34rpx}50%{height:40rpx;transform:rotate(-24deg);width:40rpx}to{transform:rotate(0)}}@keyframes thumb-bad{0%{height:34rpx;transform:rotate(0);width:34rpx}50%{height:40rpx;transform:rotate(24deg);width:40rpx}to{height:34rpx;transform:rotate(0);width:34rpx}}