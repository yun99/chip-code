<view class="guarantee-remind-container" bindtap="showPopup" tt:if="{{guaranteeRemindData}}"><view class="guarantee-remind-title">{{guaranteeRemindData.guarantee.title.text}}</view><view class="guarantee-remind-content"><wq class="guarantee-icon" webp-conf="{{({width:'92rpx', height:'30rpx'})}}" src="{{guaranteeRemindData.guarantee.icon.url}}" tt:if="{{guaranteeRemindData.guarantee.icon}}" /><view class="guarantee-txt">{{guaranteeRemindData.InlineText}}</view></view><view class="promotion-right" tt:if="{{guaranteeRemindData.guarantee.panel}}"><wq webp-conf="{{({width:'20rpx', height:'20rpx'})}}" isBlockImg="{{true}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/sku_detail_right_arrow_grey.2649732.png" /></view></view><ed show="{{showPopup}}" type="bottom" bindclose="closePopup" is-full-screen="{{true}}" catchtouchmove="prevents"><view class="popup-screen"><view class="popup-header"><view class="popup-title g-font-bold">{{guaranteeRemindData.guarantee.title.text || '平台保障'}}</view></view><scroll-view scroll-y="{{true}}" class="guarantee-area" style="max-height:{{popupMaxHeight}};"><view class="guarantee-detail-area"><block tt:for="{{guaranteeRemindData.guarantee.panel || []}}" tt:key="index"><view class="guarantee-item"><view class="first-line"><wq class="guarantee-item-icon" webp-conf="{{({width:'40rpx', height:'40rpx'})}}" src="{{item.icon.url}}" /><view class="guarantee-item-title g-font-bold">{{item.title.text}}</view></view><view class="second-line"><view class="guarantee-item-content">{{item.des.text}}</view></view></view></block></view></scroll-view><view class="selected-panel-area"><view class="confirm-btn" bindtap="closePopup" type="warn">关 闭</view></view></view></ed>