<view class="sku-recommend-container" tt:if="{{skuRecommendData}}"><view class="sku-recommend"><view class="sku-recommed-header"><view class="title g-font-bold">{{skuRecommendData.moduleTitle.text || '相似推荐'}}</view><z bid="b_youxuan_pmmzc25k_mv" lx-params="{{({sku_id: skuRecommendData.skuId, product_id: skuRecommendData.productId || ''})}}" bindmvReport="onMvReport" tt:if="{{skuRecommendData.moreUrl}}"><view class="see-more g-text-3 g-color-4" bind:tap="toMoreCategory" data-position="head">{{'查看更多'}}<wq webp-conf="{{({width:'20rpx', height:'20rpx'})}}" isBlockImg="{{true}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/right_arrow_grey.2649609.png" /></view></z></view><scroll-view class="scroll-view-x" scroll-x="true" scroll-with-animation="true" bindscroll="onScrollSkuCard"><view class="scroll-view-content {{skuRecommendData.rowType === 2 ? 'scroll-view-content-2' : ''}}" style="{{skuRecommendData.listStyle}}"><ux class="scroll-view-content-item" index="{{index}}" schema="{{skuRecommendData.schema}}" sku="{{item}}" type="{{skuType}}" mv-props="{{({bid: 'b_youxuan_lx7s3e1u_mv', mvHandler: mvHandler})}}" bind:gotoSkuDetail="gotoSkuDetail" tt:for="{{skuRecommendData.itemList}}" tt:key="skuId"></ux><z bid="b_youxuan_b4dxmdie_mv" lx-params="{{({sku_id: skuRecommendData.skuId, product_id: skuRecommendData.productId || ''})}}" bindmvReport="onMvReport" tt:if="{{skuRecommendData.moreUrl.text.length < 7}}"><view class="more-category-container {{skuRecommendData.rowType === 2 ? 'more-category-container-2' : ''}}" bind:tap="toMoreCategory"><view class="more-category-txt g-text-3 g-color-3 {{skuRecommendData.rowType === 2 ? 'more-category-txt-2' : ''}}">{{skuRecommendData.moreUrl.text || ''}}</view><wq class="jump-icon" webp-conf="{{({width:'26rpx', height:'26rpx'})}}" isBlockImg="{{true}}" src="{{'https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/more_jump_icon.f7631008.png'}}"></wq></view></z></view></scroll-view></view></view>