<wxs module="tools" src="../../jr.sjs" /><view class="more-suppliers-container" tt:if="{{multiSuppliersData}}"><z bid="b_youxuan_hhzrixjw_mv" lx-params="{{({sku_id: multiSuppliersData.skuId})}}" bindmvReport="onMvReport"><view class="more-suppliers-title"><view class="title-text">{{multiSuppliersData.sellBusinessVO.moduleTitle.text}}</view></view><view class="sku-list-container"><view class="sku-item-wrap" tt:for="{{multiSuppliersData.sellBusinessVO.itemList}}" tt:key="index" tt:for-item="skuItem" tt:for-index="index"><view class="left-area"><view class="sku-image-area"><wq webp-conf="{{({width: '90rpx', height: '90rpx'})}}" src="{{skuItem.picUrl}}"></wq><wq class="sold-out-image" webp-conf="{{({width: '90rpx', height: '90rpx'})}}" src="http://p1.meituan.net/scarlett/01a904d7e23bf375ea6c2e49086f67576097.png" tt:if="{{skuItem.soldOut}}"></wq></view><view class="sku-info-area {{skuItem.soldOut ? 'sold-out-style' : ''}}"><view class="first-line"><view class="g-price g-price-4 sku-price"><view class="g-price-unit">¥</view>{{tools.removePriceTextSymbol(skuItem.sellPrice.text)}}</view><view style="margin-left:2rpx;">/{{skuItem.sellUnit.text}}</view></view><view class="supplier-name">{{skuItem.sellBusinessName.text || ''}}</view></view></view><view class="right-area" bindtap="tapAddCartBtn" data-sold-out="{{skuItem.soldOut}}" data-selected-sku-id="{{skuItem.skuId}}" data-sell-price="{{skuItem.sellPrice.text}}"><view class="add-cart-btn {{skuItem.soldOut ? 'sold-out-style' : ''}}">加入购物车</view></view></view></view></z></view>