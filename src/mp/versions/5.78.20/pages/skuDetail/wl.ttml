<view class="feature-labels-container" tt:if="{{featureLabelsData.v3Labels}}"><block tt:for="{{featureLabelsData.v3Labels}}" tt:for-item="label" tt:key="text"><view class="feature-container" bind:tap="onTapFeatureItem" data-label-item="{{label}}"><view class="feature-icon-container" tt:if="{{label.iconUrl}}"><wq class="feature-icon" webp-conf="{{({width: '32rpx', height: '32rpx'})}}" pbgColor="#FFFFFF" src="{{label.iconUrl}}" customPlaceholder="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/sku_detail_feature-default_tag.4e92726.png" /></view><view class="feature-text-container" tt:else><view class="g-color-6 g-text-5 g-font-bold">{{label.text}}</view><view class="separator" /></view><view class="featrue-content"><view class="g-color-2 g-text-5">{{label.content}}</view></view><view class="can-click-featrue" tt:if="{{label.moreIconUrl}}"><wq webp-conf="{{({width: '28rpx', height: '28rpx'})}}" src="{{label.moreIconUrl}}" /></view></view></block></view>