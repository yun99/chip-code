<view class="sku-images-container"><swiper class="sku-image-swiper" bindchange="onSkuImageChange" current="{{skuImageData.initialSkuImageIndex}}" circular="{{true}}"><swiper-item tt:if="{{skuImageData.canDisplayVideo}}"><view class="sku-video-container"><video class="sku-video" id="skuVideo" src="{{skuImageData.video.url}}" show-mute-btn="{{true}}" enable-progress-gesture="{{false}}" show-progress="{{true}}" show-center-play-btn="{{true}}" muted="{{skuImageData.v_muted}}" poster="{{skuImageData.picUrls[0] || ''}}" bindplay="onPlaySkuVideo" bindended="onEndedSkuVideo" bindcontrolstoggle="onControlsToggle" /></view></swiper-item><swiper-item tt:for="{{skuImageData.skuImageDisplayList}}" tt:for-item="imageItem" tt:for-index="imageIndex" tt:key="*this"><view class="sku-image-wrap"><wq class="sku-image-item" lazy-load="{{false}}" mode="widthFix" webp-conf="{{({width:'750rpx', height:'750rpx', isProgressive: imageIndex === 0, progressiveWith: imageIndex === 0 ? progressiveWidth : 0 , progressiveHeight: imageIndex === 0 ? progressiveHeight : 0 })}}" src="{{imageItem}}" bindload="{{imageIndex === 0 ? 'loadFirstImgSuccess' : ''}}" binderror="{{imageIndex === 0 ? 'loadFirstImgFail' : ''}}" bindtap="tapSkuImage" data-image-index="{{imageIndex}}" data-image-item-url="{{imageItem}}" /></view></swiper-item><swiper-item tt:if="{{!skuImageData.hasContentDisplay}}"><view class="sku-image-default" /></swiper-item></swiper><view class="footer-wrap {{skuImageData.canDisplayVideo ? v_showFooter : ''}}" tt:if="{{skuImageData.hasContentDisplay}}"><view class="footer"><view class="footer-current">{{currentSkuImageIndex}}</view><view class="solidus">/</view><view class="footer-total">{{skuImageData.footerTotal}}</view></view></view></view>