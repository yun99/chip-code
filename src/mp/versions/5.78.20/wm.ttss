.flex-align-center{align-items:center;display:flex}.root{position:relative}.card-root{height:192rpx;justify-content:space-between;padding:12rpx 24rpx 12rpx 0}.card-select-box{height:100%}.card-select{padding:0 30rpx}.content{display:flex}.good-pic-box{position:relative}.card-image{margin-right:18rpx}.invalid-pic{background:rgba(0,0,0,.7);border-radius:0 0 8rpx 8rpx;bottom:0;color:hsla(0,0%,100%,.84);font-family:PingFangSC-Medium;font-size:26rpx;font-weight:700;height:40rpx;justify-content:center;line-height:38rpx;position:absolute;width:calc(100% - 12rpx);z-index:5}.card-message{display:flex;flex-direction:column;justify-content:space-between;width:calc(100vw - 314rpx)}.card-title{align-items:center;display:flex}.title{font-size:28rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;width:100%}.price{font-size:30rpx;justify-content:space-between}.final-price,.price{align-items:baseline;display:flex}.final-price{color:#f20000;font-size:36rpx;font-weight:700}.final-price-symbol{font-size:24rpx;font-weight:700;margin-right:4rpx}.seller-price{color:#95989e;font-size:24rpx;font-weight:400;margin-left:6rpx;text-decoration:line-through}.invalid-good{background:hsla(0,0%,100%,.6);height:216rpx;position:absolute;right:0;top:0;width:calc(100vw - 98rpx);z-index:10}.title-label{display:inline-block;margin-right:8rpx}