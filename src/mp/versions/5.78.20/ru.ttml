<wxs module="asset" src="./jw.sjs" /><wxs module="mtools" src="./jh.sjs" /><view class="container {{isMT ? 'container-mt': ''}}"><view class="left-area" catch:tap="setLocation"><image class="poi-icon flex-none" src="https://img.meituan.net/groceryapi/4ffdcc36fea0ffa43bc89be71c7e26091376410.png" /><wq class="delivery-img flex-none" mode="heightFix" webp-conf="{{({height: '39rpx' })}}" src="{{deliveryImg}}" tt:if="{{deliveryImg}}" /><view class="flex-none" tt:else>自提点：</view><view class="poi-name text-ellipsis {{groupGuideVO && groupGuideVO.userRole === 2 ? 'poi-name-sort' : ''}}" style="{{isRed?'color:#fff':''}}">{{poiName}}</view><view class="separator-line g-text-5 flex-none">|</view><view class="switch-text flex-none">切换</view><wq class="go-icon mr-30 flex-none" src="{{asset('images/newPoiBar/goSelect')}}" webp-conf="{{({ width: '36rpx', height: '36rpx' })}}" /></view><view class="right-area" data-share-position="{{1}}" data-share-button-object="门店" catch:tap="tapShare" tt:if="{{!hideShareBtn}}"><z customStyle="display: inline-flex" bid="{{shareBid}}" lx-params="{{mtools.shareLxParams(shareBtnPath)}}" bindmvReport="onMvReport" reportImmediately="{{true}}"><wq isBlockImg src="{{asset('images/newPoiBar/share-v2')}}" webp-conf="{{({width: '36rpx', height: '36rpx'})}}" style="margin-left: 10rpx;" /></z><view class="share-text">分享</view></view><view class="right-area" catch:tap="goGrouperLanding" tt:if="{{groupGuideVO && groupGuideVO.userRole === 2}}"><z customStyle="display: inline-flex; align-items: center;" bid="{{groupGuideBid}}" lx-params="{{({bid: groupGuideBid })}}" bind:mvReport="onMvReport"><view class="share-text text-ellipsis">{{groupGuideVO.guideDoc.groupHeaderShareText || '去分享赚钱'}}</view><wq class="go-icon" isBlockImg src="{{asset('images/newPoiBar/goSelect')}}" webp-conf="{{({ width: '18rpx', height: '18rpx' })}}" /></z></view></view>