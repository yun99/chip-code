<import src="./oq.ttml" /><tf class="coupon-detail-dialog" type="bottom" show="{{couponDetailShow}}" zIndex="{{210}}" isShowIpxBar="{{false}}" container-style="{{containerStyle}}" bind:maskclick="toHideCouponDetail" catchtouchmove="stopPropagation"><z bindmvReport="onMvReport" noData><view class="coupon-detail-dialog__container"><view class="coupon-detail-dialog__header">{{couponDetailData.floatingLayerName || '优惠券'}}</view><view class="coupon-detail-dialog__content"><scroll-view class="coupon-list" scroll-y lower-threshold="300" bindscrolltolower="toLoadMoreCouponDetail" tt:if="{{(couponDetailData && couponDetailData.couponUserList && couponDetailData.couponUserList.length) || (couponDetailData && couponDetailData.availableCouponList && couponDetailData.availableCouponList.total)}}"><block tt:if="{{couponDetailData.availableCouponList.total}}"><view class="flex-align-center tip"><text>{{couponDetailData.availableCouponTitle}}</text><wq class="flex-align-center icon-tip" webp-conf="{{({width: '24rpx',height: '24rpx'})}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-coupon-question.6f1580.png" bindtap="jumpToCouponDoc" tt:if="{{couponDetailData.couponRuleLink}}" /></view><block tt:for="{{couponDetailData.availableCouponList.couponList}}" tt:key="i"><view class="coupon-detail-dialog__best" tt:if="{{item.bestDiscount}}"><view class="flex-align-center title"><wq src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-coupon-icon.059f76.png" class="flex-align-center icon" webp-conf="{{({width: '36rpx',height: '36rpx'})}}" />{{item.bestDiscountText}}</view><view class="contain"><es pageType="cart" coupon="{{item}}" bind:use="onCouponUse"><view slot="valid-coupon" class="valid-custom" tt:if="{{item.isCanUseInCart}}">当前可用</view><view slot="desc-dynamic" class="coupon-sku-list"><scroll-view scroll-x class="desc-dynamic-scrollbar"><view class="sku" tt:for="{{item.skuList}}" tt:for-item="sku" tt:key="j"><template is="sku-item-pic" data="{{pic: sku.picUrl}}"></template><view class="sku-name">{{sku.name}}</view></view></scroll-view></view></es></view></view><es pageType="cart" coupon="{{item}}" bind:use="onCouponUse" tt:else><view slot="valid-coupon" class="valid-custom" tt:if="{{item.isCanUseInCart}}">当前可用</view><view slot="desc-dynamic" class="coupon-sku-list"><scroll-view class="desc-dynamic-scrollbar" scroll-x><view class="sku" tt:for="{{item.skuList}}" tt:for-item="sku" tt:key="j"><template is="sku-item-pic" data="{{pic: sku.picUrl}}"></template><view class="sku-name">{{sku.name}}</view></view></scroll-view></view></es></block></block><block tt:if="{{couponDetailData.couponUserList.length}}"><view class="flex-align-center tip"><text>{{couponDetailData.floatingLayerTitle}}</text><wq class="flex-align-center icon-tip" webp-conf="{{({width: '24rpx',height: '24rpx'})}}" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-coupon-question.6f1580.png" bindtap="jumpToCouponDoc" tt:if="{{couponDetailData.couponRuleLink}}" /></view><block tt:for="{{couponDetailData.couponUserList}}" tt:key="i"><view class="coupon-detail-dialog__best" tt:if="{{item.bestDiscount}}"><view class="flex-align-center title"><wq src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/cart-coupon-icon.059f76.png" class="flex-align-center icon" webp-conf="{{({width: '36rpx',height: '36rpx'})}}" />{{item.bestDiscountText}}</view><view class="contain"><es pageType="cart" coupon="{{item}}" bind:use="onCouponUse" couponIndex="{{index}}"><view slot="valid-coupon" class="valid-custom" tt:if="{{item.isCanUseInCart}}">当前可用</view><view slot="desc-dynamic" class="coupon-sku-list"><scroll-view scroll-x class="desc-dynamic-scrollbar"><view class="sku" tt:for="{{item.skuList}}" tt:for-item="sku" tt:key="j"><template is="sku-item-pic" data="{{pic: sku.picUrl}}"></template><view class="sku-name">{{sku.name}}</view></view></scroll-view></view></es></view></view><es pageType="cart" coupon="{{item}}" bind:use="onCouponUse" couponIndex="{{index}}" tt:else><view slot="valid-coupon" class="valid-custom" tt:if="{{item.isCanUseInCart}}">当前可用</view><view slot="desc-dynamic" class="coupon-sku-list"><scroll-view class="desc-dynamic-scrollbar" scroll-x><view class="sku" tt:for="{{item.skuList}}" tt:for-item="sku" tt:key="j"><template is="sku-item-pic" data="{{pic: sku.picUrl}}"></template><view class="sku-name">{{sku.name}}</view></view></scroll-view></view></es></block><view class="no-more" tt:if="{{couponDetailData.couponUserList.length === couponDetailData.total}}">没有更多优惠券了</view></block></scroll-view><view class="flex-align-center coupon-empty" tt:else><image class="coupon-empty-img" src="https://s3plus.meituan.net/v1/mss_1f3dca8fe8534e6fa640054bf3ad2ba6/grocery-fe-cdn/wx/images/cdn/no_coupon_160_160.a094473.png" /><view class="coupon-empty-txt">当前您还没有可用的优惠券哦</view></view></view><view class="coupon-detail-dialog__footer"><yh type="primary" bindtap="toHideCouponDetail">知道了</yh></view></view></z></tf>